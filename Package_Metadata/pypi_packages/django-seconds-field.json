pan class=\"kn\">from</span> <span class=\"nn\">django_seconds_field</span> <span class=\"kn\">import</span> <span class=\"n\">SecondsField</span>\n\n<span class=\"k\">class</span> <span class=\"nc\">FooBar</span><span class=\"p\">(</span><span class=\"n\">models</span><span class=\"o\">.</span><span class=\"n\">Model</span><span class=\"p\">):</span>\n    <span class=\"n\">ttl</span> <span class=\"o\">=</span> <span class=\"n\">SecondsField</span><span class=\"p\">()</span>\n</pre>\n</div>\n<div id=\"documentation\">\n<h3>Documentation</h3>\n<p>See <a href=\"http://django-seconds-field.readthedocs.org/\" rel=\"nofollow\">http://django-seconds-field.readthedocs.org/</a></p>\n</div>\n<div id=\"license\">\n<h3>License</h3>\n<p>Copyright \u00a9 2019, Piotr Roszatycki</p>\n<p>This software is distributed under the GNU Lesser General Public License (LGPL\n3 or greater).</p>\n</div>\n</div>\n\n          </div>"}, "last_serial": 5174450, "releases": {"0.1.0": [{"comment_text": "", "digests": {"md5": "85eb31399ee813870aa30167c7ff2ba3", "sha256": "d1252a3356fa284b5977aff64cb0247f3d54324a62a4bba46f47f078bd3fd7b4"}, "downloads": -1, "filename": "django_seconds_field-0.1.0-py2.py3-none-any.whl", "has_sig": false, "md5_digest": "85eb31399ee813870aa30167c7ff2ba3", "packagetype": "bdist_wheel", "python_version": "py2.py3", "requires_python": null, "size": 5422, "upload_time": "2019-04-22T20:24:53", "upload_time_iso_8601": "2019-04-22T20:24:53.809107Z", "url": "https://files.pythonhosted.org/packages/5d/ee/1add923b84c6dde4166c570f0d0d5c98aaae49350c518a12eb74235a9315/django_seconds_field-0.1.0-py2.py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "234c0a8ca246c45cbf51e870f549b958", "sha256": "ce81a3e50dccb1b83954b18cb822db64cfc58d05a8fcd4080151ab2305b87125"}, "downloads": -1, "filename": "django-seconds-field-0.1.0.tar.gz", "has_sig": false, "md5_digest": "234c0a8ca246c45cbf51e870f549b958", "packagetype": "sdist", "python_version": "source", "requires_pyth