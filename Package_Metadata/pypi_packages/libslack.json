b\n+++++++++++\n\n.. code-block:: console\n\n   pip install git+https://github.com/Nicoretti/libslack.git\n\nFrom Source\n+++++++++++\n\n.. code-block:: console\n\n    python3 setup.py install\n\n", "description_content_type": null, "docs_url": null, "download_url": "", "downloads": {"last_day": -1, "last_month": -1, "last_week": -1}, "home_page": "https://github.com/Nicoretti/libslack", "keywords": "slack,cmd,api,shell", "license": "BSD", "maintainer": "", "maintainer_email": "", "name": "libslack", "package_url": "https://pypi.org/project/libslack/", "platform": "UNKNOWN", "project_url": "https://pypi.org/project/libslack/", "project_urls": {"Homepage": "https://github.com/Nicoretti/libslack"}, "release_url": "https://pypi.org/project/libslack/1.0.0/", "requires_dist": null, "requires_python": "", "summary": "A lightweight wrapper around the slack web API.", "version": "1.0.0", "yanked": false, "html_description": "<div class=\"project-description\">\n            <div id=\"status\">\n<h2>Status</h2>\n<a href=\"https://travis-ci.org/Nicoretti/libslack\" rel=\"nofollow\"><img alt=\"https://travis-ci.org/Nicoretti/libslack.svg?branch=master\" src=\"https://warehouse-camo.ingress.cmh1.psfhosted.org/e657fc6875ce8a273506d91bae297ff91cc88eb9/68747470733a2f2f7472617669732d63692e6f72672f4e69636f72657474692f6c6962736c61636b2e7376673f6272616e63683d6d6173746572\"></a>\n<a href=\"https://coveralls.io/github/Nicoretti/libslack?branch=master\" rel=\"nofollow\"><img alt=\"https://coveralls.io/repos/Nicoretti/libslack/badge.svg?branch=master&amp;service=github\" src=\"https://warehouse-camo.ingress.cmh1.psfhosted.org/a4e1b701a3c3ac34a9730c4805f227ae96c02f4c/68747470733a2f2f636f766572616c6c732e696f2f7265706f732f4e69636f72657474692f6c6962736c61636b2f62616467652e7376673f6272616e63683d6d617374657226736572766963653d676974687562\"></a>\n<a href=\"http://libslack.readthedocs.org/en/latest/?badge=latest\" rel=\"nofollow\"><img alt=\"Documentation Status\" src=\"https://warehouse-camo.ingr