256": "93d85edb7d6d891cc83e0290ebcb7fb3ce2275ff79f9006ce1e8e9167b8a98c9"}, "downloads": -1, "filename": "octavvs-0.0.22-py3-none-any.whl", "has_sig": false, "md5_digest": "15e5f52c2b80eb69c493e331a9d2be0b", "packagetype": "bdist_wheel", "python_version": "py3", "requires_python": ">=3.6", "size": 268416, "upload_time": "2019-12-06T02:12:55", "upload_time_iso_8601": "2019-12-06T02:12:55.942884Z", "url": "https://files.pythonhosted.org/packages/fe/c3/097e2f43605dadb63b137c6a7c31d184f349417d3851ea57f8075095ac6e/octavvs-0.0.22-py3-none-any.whl", "yanked": false}], "0.0.23": [{"comment_text": "", "digests": {"md5": "89173c38b220c1dc2a2bfa3c71a2b532", "sha256": "11ce5dca7ff163a7d01e307258cf90235884ce2a962ae8eb3a571d2c530ae130"}, "downloads": -1, "filename": "octavvs-0.0.23-py3-none-any.whl", "has_sig": false, "md5_digest": "89173c38b220c1dc2a2bfa3c71a2b532", "packagetype": "bdist_wheel", "python_version": "py3", "requires_python": ">=3.6", "size": 268502, "upload_time": "2019-12-07T22:42:01", "upload_time_iso_8601": "2019-12-07T22:42:01.448327Z", "url": "https://files.pythonhosted.org/packages/6d/6f/6d4c9f7203d5ec08e8aba484d2c51bb5146db2ebbdbd5898374566bb82fc/octavvs-0.0.23-py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "76e6ba84fb94ce07cf55cbc5a8e9aede", "sha256": "0a9172f73ae2468873e067acdabfbc6c04ea3bd5ac0536005d6d37148b88b35a"}, "downloads": -1, "filename": "octavvs-0.0.23.tar.gz", "has_sig": false, "md5_digest": "76e6ba84fb94ce07cf55cbc5a8e9aede", "packagetype": "sdist", "python_version": "source", "requires_python": ">=3.6", "size": 54987, "upload_time": "2019-12-07T22:42:03", "upload_time_iso_8601": "2019-12-07T22:42:03.494145Z", "url": "https://files.pythonhosted.org/packages/98/fb/41d72962144237e7c2bd5c6c293b0e46b36370db9aaf64648056db04b0e6/octavvs-0.0.23.tar.gz", "yanked": false}], "0.0.24": [{"comment_text": "", "digests": {"md5": "7f75c065abf36a7286db5148f1ddc360", "sha256": "d54d743c49f869e8cf5f2c32acb279f7ecc0c1a5afd581e2a479dba