ary\n</pre>\n</div>\n<div id=\"usage\">\n<h2>Usage</h2>\n<pre>import monetary\n\n# Exchange Cent &amp; Dollar\nmonetary.cent(dollar, rate=100, cast_func=int)\nmonetary.dollar(cent, rate=100, cast_func=float, ndigits=None)\n\n# Mul &amp; Div\nmonetary.mul(multiplicand, multiplicator, cast_func=float)\nmonetary.div(dividend, divisor, cast_func=float, ndigits=None)\n</pre>\n</div>\n\n          </div>"}, "last_serial": 5231974, "releases": {"1.0.1": [{"comment_text": "", "digests": {"md5": "0fbf2cf359a0413664dd39921b179669", "sha256": "286c92594605368399d953b316d7e571da15d582746a297cafaf4d3f4c1705d0"}, "downloads": -1, "filename": "monetary-1.0.1-py2.py3-none-any.whl", "has_sig": false, "md5_digest": "0fbf2cf359a0413664dd39921b179669", "packagetype": "bdist_wheel", "python_version": "py2.py3", "requires_python": null, "size": 3329, "upload_time": "2018-03-21T19:55:36", "upload_time_iso_8601": "2018-03-21T19:55:36.773606Z", "url": "https://files.pythonhosted.org/packages/8f/ae/12cfc45083bc840dd3434a0b7898f06b75a59cb2a6705af2ae1892c6bc8a/monetary-1.0.1-py2.py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "15b0dcff02b0ec67bde8709ead9aea8e", "sha256": "095b3d2a7ea90ec480ed75130fcb636e115dea66a2cead836f7986e0e713b8f1"}, "downloads": -1, "filename": "monetary-1.0.1.tar.gz", "has_sig": false, "md5_digest": "15b0dcff02b0ec67bde8709ead9aea8e", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 1829, "upload_time": "2018-03-21T19:55:40", "upload_time_iso_8601": "2018-03-21T19:55:40.056145Z", "url": "https://files.pythonhosted.org/packages/ad/c6/3f26d9d4f090d45c09a39f0f733c6d0698c0f7c6f9e8474459ee2b228026/monetary-1.0.1.tar.gz", "yanked": false}], "1.0.2": [{"comment_text": "", "digests": {"md5": "577c59936e245d0742154eef5c45aabf", "sha256": "07f7f0aa09d828c9c70576e2da875b54517c5550da8ef70c78375cb58f1183c4"}, "downloads": -1, "filename": "monetary-1.0.2-py2.py3-none-any.whl", "has_sig": false, "md5_digest": "577c59936e2