alse, "md5_digest": "83e7b7347e7c7506b90cbc28b3e4bdbb", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 14383, "upload_time": "2014-02-13T21:38:54", "upload_time_iso_8601": "2014-02-13T21:38:54.276647Z", "url": "https://files.pythonhosted.org/packages/c9/08/466542f5a94e3316436ed1af8998c0705315789a72b73dc457ab3b376aab/mom-utils-1.2.6.tar.gz", "yanked": false}], "1.2.7": [{"comment_text": "", "digests": {"md5": "6d35bb3860d4eab4f8caf3cb97fc9f5f", "sha256": "bd8730b14afb8616a4211c5f50260f60b6bd91c83634feb99fc073719dafd788"}, "downloads": -1, "filename": "mom-utils-1.2.7.tar.gz", "has_sig": false, "md5_digest": "6d35bb3860d4eab4f8caf3cb97fc9f5f", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 15012, "upload_time": "2016-03-24T00:38:31", "upload_time_iso_8601": "2016-03-24T00:38:31.333210Z", "url": "https://files.pythonhosted.org/packages/f6/e3/5bf1c347f8fc35a1be3eb3540b922d33af3e764055ed4f206096e46d148c/mom-utils-1.2.7.tar.gz", "yanked": false}]}, "urls": [{"comment_text": "", "digests": {"md5": "6d35bb3860d4eab4f8caf3cb97fc9f5f", "sha256": "bd8730b14afb8616a4211c5f50260f60b6bd91c83634feb99fc073719dafd788"}, "downloads": -1, "filename": "mom-utils-1.2.7.tar.gz", "has_sig": false, "md5_digest": "6d35bb3860d4eab4f8caf3cb97fc9f5f", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 15012, "upload_time": "2016-03-24T00:38:31", "upload_time_iso_8601": "2016-03-24T00:38:31.333210Z", "url": "https://files.pythonhosted.org/packages/f6/e3/5bf1c347f8fc35a1be3eb3540b922d33af3e764055ed4f206096e46d148c/mom-utils-1.2.7.tar.gz", "yanked": false}], "timestamp": "Fri May  8 00:52:28 2020"}