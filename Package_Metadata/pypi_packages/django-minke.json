ythonhosted.org/packages/05/a4/b6991f7c6fd3ad167cd6378a64d2167ba3994ece8fa9ec675a0117d9fb94/django-minke-1.0.3.dev0.tar.gz", "yanked": false}], "1.0.4.dev0": [{"comment_text": "", "digests": {"md5": "a45dae461ca4fed4ff84dec9f99274cb", "sha256": "88b1292446cf968448129c0a8e39ddb4a8f515278f942d6051cb2d557f466924"}, "downloads": -1, "filename": "django_minke-1.0.4.dev0-py3-none-any.whl", "has_sig": false, "md5_digest": "a45dae461ca4fed4ff84dec9f99274cb", "packagetype": "bdist_wheel", "python_version": "py3", "requires_python": null, "size": 50518, "upload_time": "2019-09-04T09:43:21", "upload_time_iso_8601": "2019-09-04T09:43:21.581866Z", "url": "https://files.pythonhosted.org/packages/55/6a/ff7b8d6d0757587bdbeb04dd6cb72acdf06212ce282575de79cce9f6db4d/django_minke-1.0.4.dev0-py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "773d2dfcb7622170234d765fbd0a3f9b", "sha256": "979c3a085673605556b129e5ad4684e69dbcd0423acae88beb64f69046d12991"}, "downloads": -1, "filename": "django-minke-1.0.4.dev0.tar.gz", "has_sig": false, "md5_digest": "773d2dfcb7622170234d765fbd0a3f9b", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 35804, "upload_time": "2019-09-04T09:43:26", "upload_time_iso_8601": "2019-09-04T09:43:26.261694Z", "url": "https://files.pythonhosted.org/packages/11/2a/98ec835a9e4119ddb76835a24a37399394c6cc5fdf91b174c65994052e4c/django-minke-1.0.4.dev0.tar.gz", "yanked": false}], "1.1.0.dev0": [{"comment_text": "", "digests": {"md5": "c391bb41da399126d51e132a69648ec2", "sha256": "e788fc1e92917ccc4ac9a51978c6b2c4f44e15ffb0d0ab10e6d0a82218cb9319"}, "downloads": -1, "filename": "django_minke-1.1.0.dev0-py3-none-any.whl", "has_sig": false, "md5_digest": "c391bb41da399126d51e132a69648ec2", "packagetype": "bdist_wheel", "python_version": "py3", "requires_python": null, "size": 55516, "upload_time": "2019-11-13T13:04:33", "upload_time_iso_8601": "2019-11-13T13:04:33.939670Z", "url": "https://files.pythonhosted.org/packa