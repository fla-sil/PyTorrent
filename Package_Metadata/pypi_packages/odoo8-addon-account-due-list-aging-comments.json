_type": null, "docs_url": null, "download_url": "", "downloads": {"last_day": -1, "last_month": -1, "last_week": -1}, "home_page": "http://www.eficent.com", "keywords": "", "license": "AGPL-3", "maintainer": "", "maintainer_email": "", "name": "odoo8-addon-account-due-list-aging-comments", "package_url": "https://pypi.org/project/odoo8-addon-account-due-list-aging-comments/", "platform": "", "project_url": "https://pypi.org/project/odoo8-addon-account-due-list-aging-comments/", "project_urls": {"Homepage": "http://www.eficent.com"}, "release_url": "https://pypi.org/project/odoo8-addon-account-due-list-aging-comments/8.0.0.2.0.99.dev10/", "requires_dist": ["odoo (<9.0a,>=8.0a)", "odoo8-addon-account-due-list"], "requires_python": "~=2.7", "summary": "Payments Due list aging comments", "version": "8.0.0.2.0.99.dev10", "yanked": false, "html_description": "<div class=\"project-description\">\n            <a href=\"http://www.gnu.org/licenses/agpl-3.0-standalone.html\" rel=\"nofollow\"><img alt=\"License: AGPL-3\" src=\"https://warehouse-camo.ingress.cmh1.psfhosted.org/e275d7ebc120b0c2ff730ee763f7471f965e9298/68747470733a2f2f696d672e736869656c64732e696f2f62616467652f6c6963656e63652d4147504c2d2d332d626c75652e737667\"></a>\n<div id=\"account-due-list-aging-comments\">\n<h2>Account Due List Aging Comments</h2>\n<p>This module extends the Payments and Due list to add the capability to\nintroduce aging comments in-line.</p>\n<div id=\"configuration\">\n<h3>Configuration</h3>\n<p>No configuration is needed.</p>\n</div>\n<div id=\"usage\">\n<h3>Usage</h3>\n<p>To use this module, you need to go to:</p>\n<ul>\n<li>Accounting-&gt;Journal Entries-&gt;Payments and due list</li>\n</ul>\n<a href=\"https://runbot.odoo-community.org/runbot/96/8.0\" rel=\"nofollow\"><img alt=\"Try me on Runbot\" src=\"https://warehouse-camo.ingress.cmh1.psfhosted.org/dbdbc606b84d2a8e7702f02bd61c256d7bad12f0/68747470733a2f2f6f646f6f2d636f6d6d756e6974792e6f72672f776562736974652f696d6167652f69722e617474616