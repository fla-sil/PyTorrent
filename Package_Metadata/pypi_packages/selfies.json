ersion": "py3", "requires_python": null, "size": 15808, "upload_time": "2019-10-01T09:30:49", "upload_time_iso_8601": "2019-10-01T09:30:49.693486Z", "url": "https://files.pythonhosted.org/packages/01/67/f8f04deb5bc03b0ff8c7c994802620e62257373c56169485c7465c82235e/selfies-0.2.4-py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "3b07f2f557cc49a0862c5976e8dc7f2b", "sha256": "d14b15eeb611213d53c56c9aeba8899534090f88e13d0e338ee335621558c4c3"}, "downloads": -1, "filename": "selfies-0.2.4.tar.gz", "has_sig": false, "md5_digest": "3b07f2f557cc49a0862c5976e8dc7f2b", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 15775, "upload_time": "2019-10-01T09:30:51", "upload_time_iso_8601": "2019-10-01T09:30:51.134861Z", "url": "https://files.pythonhosted.org/packages/5b/21/a0edb0e3002fe1b2878056b8368e3d19cd4ee19eed84378f8674eb944edf/selfies-0.2.4.tar.gz", "yanked": false}], "timestamp": "Fri May  8 02:55:26 2020"}