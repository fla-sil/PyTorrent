load_time": "2012-12-13T10:13:00", "upload_time_iso_8601": "2012-12-13T10:13:00.095827Z", "url": "https://files.pythonhosted.org/packages/01/e8/9a6d0cb1b686833862742975bee043bcb5efc3f65269a8c677e139032336/dploi-fabric-0.1.15.tar.gz", "yanked": false}], "0.1.16": [{"comment_text": "", "digests": {"md5": "e06ebc5ee01a219498316fa67f41a77c", "sha256": "cf4ee919e36a6acb9c3959ec60890c251a0b555443c42418c8205de9e306aeb5"}, "downloads": -1, "filename": "dploi-fabric-0.1.16.tar.gz", "has_sig": false, "md5_digest": "e06ebc5ee01a219498316fa67f41a77c", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 15878, "upload_time": "2012-12-13T12:05:06", "upload_time_iso_8601": "2012-12-13T12:05:06.302919Z", "url": "https://files.pythonhosted.org/packages/fc/ca/edef4d45eb2a28bf04e88e988d1e94c0f001cf0962c4ad0f08a800b11ba2/dploi-fabric-0.1.16.tar.gz", "yanked": false}], "0.1.17": [{"comment_text": "", "digests": {"md5": "5f8ae6bc212cb4e03eeeedf92096d4ef", "sha256": "f7df20574a295dea4689880924b3f51d8ededead235b3cd3da2ec538b0fa3be7"}, "downloads": -1, "filename": "dploi-fabric-0.1.17.tar.gz", "has_sig": false, "md5_digest": "5f8ae6bc212cb4e03eeeedf92096d4ef", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 16133, "upload_time": "2012-12-18T13:55:48", "upload_time_iso_8601": "2012-12-18T13:55:48.634844Z", "url": "https://files.pythonhosted.org/packages/59/41/8c81f8b7e493a24047feca4c15ef29067f52d06c448a0f4e369d5f30c8ef/dploi-fabric-0.1.17.tar.gz", "yanked": false}], "0.1.18": [{"comment_text": "", "digests": {"md5": "440de5e9e50aaa98bb3b4c389e63b0fe", "sha256": "aaa34eb2db43e0b6835855b5e58597767a4ae9078fac493fb3da6d23427a1626"}, "downloads": -1, "filename": "dploi-fabric-0.1.18.tar.gz", "has_sig": false, "md5_digest": "440de5e9e50aaa98bb3b4c389e63b0fe", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 16216, "upload_time": "2013-01-11T14:06:05", "upload_time_iso_8601": "2013-01-11T14:06: