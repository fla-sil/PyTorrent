se}], "0.95": [{"comment_text": "", "digests": {"md5": "b58d39071f5d061c5bd1e4187d38251d", "sha256": "ddb2df352e6ea7808e0b8f65ad02b30d837ff3e29c333ea7bdfdcca891a8cd42"}, "downloads": -1, "filename": "dupliganger-0.95-py2.py3-none-any.whl", "has_sig": false, "md5_digest": "b58d39071f5d061c5bd1e4187d38251d", "packagetype": "bdist_wheel", "python_version": "py2.py3", "requires_python": ">=2.7, !=3.0.*, !=3.1.*, !=3.2.*, !=3.3.*, <4", "size": 61534, "upload_time": "2019-01-03T01:14:41", "upload_time_iso_8601": "2019-01-03T01:14:41.433326Z", "url": "https://files.pythonhosted.org/packages/54/54/baf32280191f60cc4ba02e2ec2b40720294dddad427ab2e483119cb9febc/dupliganger-0.95-py2.py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "304dbd12d41920523ef5f410c0ab3df1", "sha256": "251c543a2683cec23a64d3cc00d0492b55391b1bdd4a731a8752137bd94a4237"}, "downloads": -1, "filename": "dupliganger-0.95.tar.gz", "has_sig": false, "md5_digest": "304dbd12d41920523ef5f410c0ab3df1", "packagetype": "sdist", "python_version": "source", "requires_python": ">=2.7, !=3.0.*, !=3.1.*, !=3.2.*, !=3.3.*, <4", "size": 76004, "upload_time": "2019-01-03T01:14:43", "upload_time_iso_8601": "2019-01-03T01:14:43.498447Z", "url": "https://files.pythonhosted.org/packages/d5/08/0cbe6bc97daa79311af8fc32f4375e9e590654017d5464cc4dfdc46fd78f/dupliganger-0.95.tar.gz", "yanked": false}], "0.96": [{"comment_text": "", "digests": {"md5": "dfa0605c8ac4e3320899ad92b772811b", "sha256": "c11c137611c5fc9177dfebbeb9c41e339a4d63fa3ab5a5eed36edb11d5f918fd"}, "downloads": -1, "filename": "dupliganger-0.96-py2.py3-none-any.whl", "has_sig": false, "md5_digest": "dfa0605c8ac4e3320899ad92b772811b", "packagetype": "bdist_wheel", "python_version": "py2.py3", "requires_python": ">=2.7, !=3.0.*, !=3.1.*, !=3.2.*, !=3.3.*, <4", "size": 61532, "upload_time": "2019-01-03T01:45:25", "upload_time_iso_8601": "2019-01-03T01:45:25.423607Z", "url": "https://files.pythonhosted.org/packages/d3/6b/e10cd9cb3a895a2219564370c1