mment_text": "", "digests": {"md5": "492e2757ee5fd3f933b87861953dfabe", "sha256": "f031da5ab0baf394575913e6f73be3b1ad9c45c25480924d86a6526f7af0cf82"}, "downloads": -1, "filename": "spotifytracker-0.0.15-py3-none-any.whl", "has_sig": false, "md5_digest": "492e2757ee5fd3f933b87861953dfabe", "packagetype": "bdist_wheel", "python_version": "3.5", "requires_python": null, "size": 8721, "upload_time": "2017-05-25T18:16:45", "upload_time_iso_8601": "2017-05-25T18:16:45.009665Z", "url": "https://files.pythonhosted.org/packages/d7/d9/7aaf3117a5cbe91851c9e7fa9d5dd66fd8335c3182762d92bf3e7995d6a0/spotifytracker-0.0.15-py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "03c6185686545d16be9e28631ed6f52f", "sha256": "6ddcc0e1d4c11792b0bb11407d71c4f74d7670a3fd9d8f8e26a5205f1b849dfe"}, "downloads": -1, "filename": "spotifytracker-0.0.15.tar.gz", "has_sig": false, "md5_digest": "03c6185686545d16be9e28631ed6f52f", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 5149, "upload_time": "2017-05-25T18:16:42", "upload_time_iso_8601": "2017-05-25T18:16:42.208341Z", "url": "https://files.pythonhosted.org/packages/42/11/1e5d3a0f4da7f7daf203084d77516e9524748a4c648bc3b93bf1814f1993/spotifytracker-0.0.15.tar.gz", "yanked": false}], "0.0.16": [{"comment_text": "", "digests": {"md5": "25b07391f5822c2b83f96a59a0aadca0", "sha256": "27825ad75bec7bf902a1b45c172b3d5821886b64e3a692318991be669723076c"}, "downloads": -1, "filename": "spotifytracker-0.0.16-py3-none-any.whl", "has_sig": false, "md5_digest": "25b07391f5822c2b83f96a59a0aadca0", "packagetype": "bdist_wheel", "python_version": "3.7", "requires_python": null, "size": 7674, "upload_time": "2018-09-13T19:34:17", "upload_time_iso_8601": "2018-09-13T19:34:17.059043Z", "url": "https://files.pythonhosted.org/packages/c0/84/8724791deba59ed2a30e0409afb93fda4a95e76d17a96707f20994781c85/spotifytracker-0.0.16-py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "4c9936bee