], "0.9.5b1": [{"comment_text": "", "digests": {"md5": "a5bc15496e521b41b7cc42b0b63a40d7", "sha256": "8b1ecc03cf619ed8324522fa853e1e0f7859c4004e12fe3135a6b087b56d1f2b"}, "downloads": -1, "filename": "nive-0.9.5b1.tar.gz", "has_sig": false, "md5_digest": "a5bc15496e521b41b7cc42b0b63a40d7", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 1247116, "upload_time": "2012-11-09T17:11:08", "upload_time_iso_8601": "2012-11-09T17:11:08.449476Z", "url": "https://files.pythonhosted.org/packages/81/96/4f478b0aa1412d0e011c8586a5cd4b1873991b77f86a5d40d0f93cd6e00c/nive-0.9.5b1.tar.gz", "yanked": false}, {"comment_text": "", "digests": {"md5": "c45f1c05acd630c4b6e52935db3d52a2", "sha256": "e47729c9bcd76e8b66d91b90f31a4d18f828dfa30c11603285a4541841b92798"}, "downloads": -1, "filename": "nive-0.9.5b1.zip", "has_sig": false, "md5_digest": "c45f1c05acd630c4b6e52935db3d52a2", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 1582387, "upload_time": "2012-11-09T17:11:04", "upload_time_iso_8601": "2012-11-09T17:11:04.351522Z", "url": "https://files.pythonhosted.org/packages/eb/81/24bdc023bcf374bc8f6103b311cbaa9f8762c031e7f52649f4db3d304f8e/nive-0.9.5b1.zip", "yanked": false}], "0.9.5b2": [{"comment_text": "", "digests": {"md5": "b8a8661719aece8dd6027fc789dd06e2", "sha256": "5b79948df29eb9e28bf8cb86c80598d6c4280f9ebcd22ec2f8a3b031bc520098"}, "downloads": -1, "filename": "nive-0.9.5b2.tar.gz", "has_sig": false, "md5_digest": "b8a8661719aece8dd6027fc789dd06e2", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 1250655, "upload_time": "2013-02-02T18:50:37", "upload_time_iso_8601": "2013-02-02T18:50:37.773309Z", "url": "https://files.pythonhosted.org/packages/41/94/fe490c418c50d54e9934d747e49d884ae5d3b533f44ad63c4e0f4710ff58/nive-0.9.5b2.tar.gz", "yanked": false}, {"comment_text": "", "digests": {"md5": "9c25810da364cafffb9081ce076d0ce1", "sha256": "cccbf28605eb19e6a4e790f5752fba607f53378a