mment_text": "", "digests": {"md5": "ca39c4755b65b62395b3263737ce9fd7", "sha256": "5e3159750c8cb245263d480dd1aeb7d37d6582a677c5ae441cff26d63fb68f18"}, "downloads": -1, "filename": "nts-skazazes-0.0.12.tar.gz", "has_sig": false, "md5_digest": "ca39c4755b65b62395b3263737ce9fd7", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 5805, "upload_time": "2019-03-08T03:58:26", "upload_time_iso_8601": "2019-03-08T03:58:26.900453Z", "url": "https://files.pythonhosted.org/packages/17/29/8f35bcffec40231b218416f0779d83d06fc519707f9f31c68441278f1886/nts-skazazes-0.0.12.tar.gz", "yanked": false}], "0.0.13": [{"comment_text": "", "digests": {"md5": "932a1b7d1f5bf464f261462dc198556b", "sha256": "932a114422ac1d3b893e0af1a69a5bcbec08b515671b7259b2a07d8ba685ed15"}, "downloads": -1, "filename": "nts_skazazes-0.0.13-py3-none-any.whl", "has_sig": false, "md5_digest": "932a1b7d1f5bf464f261462dc198556b", "packagetype": "bdist_wheel", "python_version": "py3", "requires_python": null, "size": 20106, "upload_time": "2019-03-08T04:03:34", "upload_time_iso_8601": "2019-03-08T04:03:34.196245Z", "url": "https://files.pythonhosted.org/packages/f5/56/bbe92ae88a862e188e16d709fd7f9f02b78a314d8f4db93c88c41563f804/nts_skazazes-0.0.13-py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "fa5b0494ffa97fff5355aedc18a8d64c", "sha256": "2a82bd99fbb4b4bb4f1d67f608d28a987b47fc40b103a70645594a4b7913b7bf"}, "downloads": -1, "filename": "nts-skazazes-0.0.13.tar.gz", "has_sig": false, "md5_digest": "fa5b0494ffa97fff5355aedc18a8d64c", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 5834, "upload_time": "2019-03-08T04:03:35", "upload_time_iso_8601": "2019-03-08T04:03:35.494791Z", "url": "https://files.pythonhosted.org/packages/59/9d/d127cc95c1b2ed085414e665f64b6d5e8690d3610d8ef0fd03c5cf7a913f/nts-skazazes-0.0.13.tar.gz", "yanked": false}], "0.0.14": [{"comment_text": "", "digests": {"md5": "e6e861f0ff3a2b92fb5e934429e016b