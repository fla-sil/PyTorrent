t_type": "", "docs_url": null, "download_url": "", "downloads": {"last_day": -1, "last_month": -1, "last_week": -1}, "home_page": "http://www.domatix.com", "keywords": "", "license": "AGPL-3", "maintainer": "", "maintainer_email": "", "name": "odoo8-addon-contract-payment-mode", "package_url": "https://pypi.org/project/odoo8-addon-contract-payment-mode/", "platform": "", "project_url": "https://pypi.org/project/odoo8-addon-contract-payment-mode/", "project_urls": {"Homepage": "http://www.domatix.com"}, "release_url": "https://pypi.org/project/odoo8-addon-contract-payment-mode/8.0.1.0.0.99.dev20/", "requires_dist": ["odoo (<9.0a,>=8.0a)", "odoo8-addon-account-payment-partner"], "requires_python": "~=2.7", "summary": "Payment mode in contracts and their invoices", "version": "8.0.1.0.0.99.dev20", "yanked": false, "html_description": "<div class=\"project-description\">\n            <a href=\"http://www.gnu.org/licenses/agpl-3.0-standalone.html\" rel=\"nofollow\"><img alt=\"License: AGPL-3\" src=\"https://warehouse-camo.ingress.cmh1.psfhosted.org/e275d7ebc120b0c2ff730ee763f7471f965e9298/68747470733a2f2f696d672e736869656c64732e696f2f62616467652f6c6963656e63652d4147504c2d2d332d626c75652e737667\"></a>\n<div id=\"contract-payment-mode\">\n<h2>Contract Payment Mode</h2>\n<p>This module allows to set a payment mode on contract for creating the invoices with this payment mode.</p>\n<div id=\"usage\">\n<h3>Usage</h3>\n<a href=\"https://runbot.odoo-community.org/runbot/110/8.0\" rel=\"nofollow\"><img alt=\"Try me on Runbot\" src=\"https://warehouse-camo.ingress.cmh1.psfhosted.org/dbdbc606b84d2a8e7702f02bd61c256d7bad12f0/68747470733a2f2f6f646f6f2d636f6d6d756e6974792e6f72672f776562736974652f696d6167652f69722e6174746163686d656e742f353738345f663238313362642f6461746173\"></a>\n<p>For further information, please visit:</p>\n<ul>\n<li><a href=\"https://www.odoo.com/forum/help-1\" rel=\"nofollow\">https://www.odoo.com/forum/help-1</a></li>\n</ul>\n</div>\n<div id=\"bug-tracker\">\n<h3>