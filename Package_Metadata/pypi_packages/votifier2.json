   <span class=\"n\">v</span> <span class=\"o\">=</span> <span class=\"n\">Vote</span><span class=\"p\">(</span><span class=\"s2\">\"SERVICE\"</span><span class=\"p\">,</span> <span class=\"s2\">\"ano95\"</span><span class=\"p\">,</span> <span class=\"s2\">\"127.0.0.1\"</span><span class=\"p\">)</span>\n    <span class=\"n\">s</span> <span class=\"o\">=</span> <span class=\"n\">Server</span><span class=\"p\">(</span><span class=\"s2\">\"127.0.0.1\"</span><span class=\"p\">,</span> <span class=\"mi\">8192</span><span class=\"p\">,</span> <span class=\"s2\">\"TOKEN\"</span><span class=\"p\">)</span>\n    <span class=\"n\">s</span><span class=\"o\">.</span><span class=\"n\">send_vote</span><span class=\"p\">(</span><span class=\"n\">v</span><span class=\"p\">)</span>\n</pre>\n\n          </div>"}, "last_serial": 6554230, "releases": {"0.1.0": [{"comment_text": "", "digests": {"md5": "cf7136c17d5be36a561e976c48c85790", "sha256": "08d279a9e9740cec41add2633e2052bc3711de53e298bfa74428442857dfd8f2"}, "downloads": -1, "filename": "votifier2-0.1.0-py2.py3-none-any.whl", "has_sig": false, "md5_digest": "cf7136c17d5be36a561e976c48c85790", "packagetype": "bdist_wheel", "python_version": "py2.py3", "requires_python": ">=3.6", "size": 3708, "upload_time": "2020-02-01T13:02:04", "upload_time_iso_8601": "2020-02-01T13:02:04.539808Z", "url": "https://files.pythonhosted.org/packages/07/53/97c23880b89dd7d8a95c5985c1d7a046d63212a2a7029b3a3abece761cf3/votifier2-0.1.0-py2.py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "629538c9c052d88e7f5b42a9828667ef", "sha256": "730bff728ba58392ea39df7dc02db8356d78bb5d70078009fe4355a5c1287673"}, "downloads": -1, "filename": "votifier2-0.1.0.tar.gz", "has_sig": false, "md5_digest": "629538c9c052d88e7f5b42a9828667ef", "packagetype": "sdist", "python_version": "source", "requires_python": ">=3.6", "size": 2947, "upload_time": "2020-02-01T13:02:06", "upload_time_iso_8601": "2020-02-01T13:02:06.856629Z", "url": "https://files.py