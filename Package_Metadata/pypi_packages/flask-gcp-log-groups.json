kages/68/38/3764350dd6d8105ba1831e6b7bb8f09f461f615d44df4725234af3a2928d/flask-gcp-log-groups-0.0.9.tar.gz", "yanked": false}], "0.1.0": [{"comment_text": "", "digests": {"md5": "a61789720f41ec0b1cb2802ec14c7280", "sha256": "6dd83234e3163bc575652f052d6c128cf8952e09a69e48947d6cfeebeffa1137"}, "downloads": -1, "filename": "flask-gcp-log-groups-0.1.0.tar.gz", "has_sig": false, "md5_digest": "a61789720f41ec0b1cb2802ec14c7280", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 5842, "upload_time": "2018-11-07T17:45:25", "upload_time_iso_8601": "2018-11-07T17:45:25.463330Z", "url": "https://files.pythonhosted.org/packages/a6/9d/12c1ab891451a59c890c96230bbe3405a256131eb9ac61384e2f2e7ceb15/flask-gcp-log-groups-0.1.0.tar.gz", "yanked": false}]}, "urls": [{"comment_text": "", "digests": {"md5": "a61789720f41ec0b1cb2802ec14c7280", "sha256": "6dd83234e3163bc575652f052d6c128cf8952e09a69e48947d6cfeebeffa1137"}, "downloads": -1, "filename": "flask-gcp-log-groups-0.1.0.tar.gz", "has_sig": false, "md5_digest": "a61789720f41ec0b1cb2802ec14c7280", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 5842, "upload_time": "2018-11-07T17:45:25", "upload_time_iso_8601": "2018-11-07T17:45:25.463330Z", "url": "https://files.pythonhosted.org/packages/a6/9d/12c1ab891451a59c890c96230bbe3405a256131eb9ac61384e2f2e7ceb15/flask-gcp-log-groups-0.1.0.tar.gz", "yanked": false}], "timestamp": "Fri May  8 01:02:50 2020"}