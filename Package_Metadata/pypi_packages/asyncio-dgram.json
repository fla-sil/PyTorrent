e": "sdist", "python_version": "source", "requires_python": ">=3.7", "size": 8234, "upload_time": "2019-09-30T18:53:55", "upload_time_iso_8601": "2019-09-30T18:53:55.330777Z", "url": "https://files.pythonhosted.org/packages/7f/12/a4cc3bee2f83a10c9fa738191c939de321a1f14ba2784675535d3b51a108/asyncio-dgram-0.1.1.tar.gz", "yanked": false}], "0.1.2": [{"comment_text": "", "digests": {"md5": "b1e8742c9cab9c132d34c85d321617a6", "sha256": "2415e461bdecbfd568b15fbd761fb7acf7ee83b7a75ad257768fa7b116e62d4e"}, "downloads": -1, "filename": "asyncio_dgram-0.1.2-py3-none-any.whl", "has_sig": false, "md5_digest": "b1e8742c9cab9c132d34c85d321617a6", "packagetype": "bdist_wheel", "python_version": "py3", "requires_python": ">=3.6", "size": 5780, "upload_time": "2019-10-08T17:11:03", "upload_time_iso_8601": "2019-10-08T17:11:03.976274Z", "url": "https://files.pythonhosted.org/packages/15/1d/7aa990cf8e007d6a46a9e051b6c590cc8ab6e7918f6e6bfac92e0b52e873/asyncio_dgram-0.1.2-py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "ed48ae36b13039b6c870a70db260e05c", "sha256": "53e622e1b5eeeefced336f593b0027b4892f5faf833fcf38e17eb0cbe4f5f0ab"}, "downloads": -1, "filename": "asyncio-dgram-0.1.2.tar.gz", "has_sig": false, "md5_digest": "ed48ae36b13039b6c870a70db260e05c", "packagetype": "sdist", "python_version": "source", "requires_python": ">=3.6", "size": 8290, "upload_time": "2019-10-08T17:10:41", "upload_time_iso_8601": "2019-10-08T17:10:41.033363Z", "url": "https://files.pythonhosted.org/packages/37/40/4000ac0fdd0cda1edc3ddfa6d23b2e2e0ff522e465e92f12563ff8eac5f6/asyncio-dgram-0.1.2.tar.gz", "yanked": false}], "0.2.0": [{"comment_text": "", "digests": {"md5": "20731b0884afbf17415b6232d9b95c14", "sha256": "57a89db3f2038840ea3854188dca15c9b3301917aecc60eb2bbc8907a8d993bd"}, "downloads": -1, "filename": "asyncio_dgram-0.2.0-py3-none-any.whl", "has_sig": false, "md5_digest": "20731b0884afbf17415b6232d9b95c14", "packagetype": "bdist_wheel", "python_version": "py3", "require