gests": {"md5": "fbeaea075964f58e5f1939f6ff04a355", "sha256": "f6813c1bcd4eaf87ba9a455d619f5d85cf9fb6c52b216e0a66351050b246dd5c"}, "downloads": -1, "filename": "django-document-library-0.6.1.tar.gz", "has_sig": false, "md5_digest": "fbeaea075964f58e5f1939f6ff04a355", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 18683, "upload_time": "2013-05-14T08:02:34", "upload_time_iso_8601": "2013-05-14T08:02:34.788968Z", "url": "https://files.pythonhosted.org/packages/a4/55/9ed5ebe9d8feb4ba6e06ac15169bbb5f33bddc6f58ab27bd3978769b1d64/django-document-library-0.6.1.tar.gz", "yanked": false}], "0.6.2": [{"comment_text": "", "digests": {"md5": "4df5feed1cf9b996b7572591a43e241b", "sha256": "b3b4e58ac62ac6c5dbf1fc8d1254099261dd27f3dc4644d2637ac14d9844d550"}, "downloads": -1, "filename": "django-document-library-0.6.2.tar.gz", "has_sig": false, "md5_digest": "4df5feed1cf9b996b7572591a43e241b", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 18747, "upload_time": "2013-05-22T07:46:32", "upload_time_iso_8601": "2013-05-22T07:46:32.270216Z", "url": "https://files.pythonhosted.org/packages/c7/00/6771669a746081e087f49f375058d262dfec772c21bb004c1c6cc8ba2997/django-document-library-0.6.2.tar.gz", "yanked": false}], "0.6.3": [{"comment_text": "", "digests": {"md5": "1b72999fac67f3654390802b4e0393ec", "sha256": "e017bc4e16a2c587c7fa8b77281df5f61a7c0a97e20fe62dfc27a39c6253176a"}, "downloads": -1, "filename": "django-document-library-0.6.3.tar.gz", "has_sig": false, "md5_digest": "1b72999fac67f3654390802b4e0393ec", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 19247, "upload_time": "2013-06-25T12:05:55", "upload_time_iso_8601": "2013-06-25T12:05:55.767886Z", "url": "https://files.pythonhosted.org/packages/23/12/3bbf5975a364ef053ddf2075dfe612468b84159f1d8a6e2f27f8b06ad95a/django-document-library-0.6.3.tar.gz", "yanked": false}], "0.7": [{"comment_text": "", "digests": {"md5": "8e9bd