}, "downloads": -1, "filename": "djangoshop-stripe-0.12.2.tar.gz", "has_sig": false, "md5_digest": "9c2ddfee6c2d202901fe732bf0a7de71", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 24510, "upload_time": "2017-11-22T21:20:42", "upload_time_iso_8601": "2017-11-22T21:20:42.149742Z", "url": "https://files.pythonhosted.org/packages/ac/54/d22b2e2c858044877221850a947b0ab0e0d3dd7dfe2fd166ac81cfca08fc/djangoshop-stripe-0.12.2.tar.gz", "yanked": false}], "0.12.3": [{"comment_text": "", "digests": {"md5": "1f0b8ec890815a1f403123a856bdc413", "sha256": "9f70344c3ae8c80eb480c6dcba100448906376a60d5a266f346ab7121c05a482"}, "downloads": -1, "filename": "djangoshop-stripe-0.12.3.tar.gz", "has_sig": false, "md5_digest": "1f0b8ec890815a1f403123a856bdc413", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 26052, "upload_time": "2018-02-20T21:24:11", "upload_time_iso_8601": "2018-02-20T21:24:11.618935Z", "url": "https://files.pythonhosted.org/packages/1a/71/d79e30acefea8ba461847bf36abedddb443bd1e4b2655824444631febcbb/djangoshop-stripe-0.12.3.tar.gz", "yanked": false}], "0.2.0": [{"comment_text": "", "digests": {"md5": "d69f31d9aec928521487eb874a968514", "sha256": "9fd37fe1108be951b067928f0d325dccb86324eb52b42d99a66c0990a75d1ba4"}, "downloads": -1, "filename": "djangoshop-stripe-0.2.0.tar.gz", "has_sig": false, "md5_digest": "d69f31d9aec928521487eb874a968514", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 23226, "upload_time": "2016-09-04T19:54:30", "upload_time_iso_8601": "2016-09-04T19:54:30.899184Z", "url": "https://files.pythonhosted.org/packages/21/19/af2a1e804ade70aa532514f1a15a7a0978b8a45bf7566568c11b80edeabb/djangoshop-stripe-0.2.0.tar.gz", "yanked": false}], "0.2.1": [{"comment_text": "", "digests": {"md5": "af9a9eda55473e992029903ebd47b55f", "sha256": "7f75bc93afb82a0cbfd4faa52ec6b4a38658d292787dbc569340612aa659f627"}, "downloads": -1, "filename": "djangoshop-stripe-