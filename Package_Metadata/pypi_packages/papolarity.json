se}], "0.0.3.post12": [{"comment_text": "", "digests": {"md5": "953278917b281abc3eabfe5373ee4f0f", "sha256": "8c93a7781d6d048d07d3833de57c6e5eb25fee81789edc131aa783d09d2b90ea"}, "downloads": -1, "filename": "papolarity-0.0.3.post12-py2.py3-none-any.whl", "has_sig": false, "md5_digest": "953278917b281abc3eabfe5373ee4f0f", "packagetype": "bdist_wheel", "python_version": "py2.py3", "requires_python": ">=3.7", "size": 35364, "upload_time": "2020-01-22T13:43:40", "upload_time_iso_8601": "2020-01-22T13:43:40.619768Z", "url": "https://files.pythonhosted.org/packages/13/63/efff202fcf0d7c35d4ad451137d27e21f625d78ccee5b11d66d6ac1abdc0/papolarity-0.0.3.post12-py2.py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "18408d4b030237e9b5e546f3584b94ec", "sha256": "7284ceffca0a8b2276e8f70c66b7dcf6278c315c2f6ca6b8f181c567e97946cb"}, "downloads": -1, "filename": "papolarity-0.0.3.post12.tar.gz", "has_sig": false, "md5_digest": "18408d4b030237e9b5e546f3584b94ec", "packagetype": "sdist", "python_version": "source", "requires_python": ">=3.7", "size": 27828, "upload_time": "2020-01-22T13:43:42", "upload_time_iso_8601": "2020-01-22T13:43:42.745165Z", "url": "https://files.pythonhosted.org/packages/88/84/2f619e5e1a888e71bdf441114cec0d0cfd166f49fa12795f4c76cb01d14d/papolarity-0.0.3.post12.tar.gz", "yanked": false}], "0.0.3.post2": [{"comment_text": "", "digests": {"md5": "ad0d2f463b13186b9367e602d9137ec4", "sha256": "5107cff358c02faa2b14cb04b1db3305602685b298980b88535749048d06b5a1"}, "downloads": -1, "filename": "papolarity-0.0.3.post2-py2.py3-none-any.whl", "has_sig": false, "md5_digest": "ad0d2f463b13186b9367e602d9137ec4", "packagetype": "bdist_wheel", "python_version": "py2.py3", "requires_python": ">=3.7, <3.8", "size": 33240, "upload_time": "2020-01-17T14:44:01", "upload_time_iso_8601": "2020-01-17T14:44:01.413370Z", "url": "https://files.pythonhosted.org/packages/b5/cb/8159c14b9469c56df64c0fd999aaa7c208cf0c5bd3cc799c1d2d2b3a8507/papolarity-0.0.3.post2-py2.py3