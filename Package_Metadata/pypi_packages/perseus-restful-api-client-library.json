/packages/83/2b/67c10bb19b4da7f930ee1292c6f971765f30bcb9f53868b74e7f9596caf1/perseus_restful_api_client_library-1.3.0-py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "52d6a2713db58deeedfe59d3bad6dc74", "sha256": "8b75fdaeaf7235de9e89ba8a7e3865fdfa5e4999737169811c2d6db8bca425c0"}, "downloads": -1, "filename": "perseus-restful-api-client-library-1.3.0.tar.gz", "has_sig": false, "md5_digest": "52d6a2713db58deeedfe59d3bad6dc74", "packagetype": "sdist", "python_version": "source", "requires_python": ">=3", "size": 29754, "upload_time": "2019-12-24T08:56:58", "upload_time_iso_8601": "2019-12-24T08:56:58.073052Z", "url": "https://files.pythonhosted.org/packages/2a/1f/c6d0e53b7f03c930c63130ccc70566873ac411c44b54f7a0d0f92f8a7e8f/perseus-restful-api-client-library-1.3.0.tar.gz", "yanked": false}], "1.3.1": [{"comment_text": "", "digests": {"md5": "ce1597426ef5326ba1de5bcc192b0394", "sha256": "55770a111f646213882b3d515025642588fd47fd78f13c359bf13b5db02c7ef7"}, "downloads": -1, "filename": "perseus_restful_api_client_library-1.3.1-py3-none-any.whl", "has_sig": false, "md5_digest": "ce1597426ef5326ba1de5bcc192b0394", "packagetype": "bdist_wheel", "python_version": "py3", "requires_python": ">=3", "size": 45886, "upload_time": "2019-12-24T09:06:21", "upload_time_iso_8601": "2019-12-24T09:06:21.583617Z", "url": "https://files.pythonhosted.org/packages/5f/95/4f40b414f12d829ba6e2685ccf1f2cbfa5d1b6a708f942a4ea2422a3b980/perseus_restful_api_client_library-1.3.1-py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "c41225f04694e18dfcd96cd359ae81fa", "sha256": "00073caa5417c2027f5d696fa3ae91b6f20ec2ef3f06051053e9482b3498b05c"}, "downloads": -1, "filename": "perseus-restful-api-client-library-1.3.1.tar.gz", "has_sig": false, "md5_digest": "c41225f04694e18dfcd96cd359ae81fa", "packagetype": "sdist", "python_version": "source", "requires_python": ">=3", "size": 29741, "upload_time": "2019-12-24T09:06:23", "upload_time_iso_8601": "2019-12-