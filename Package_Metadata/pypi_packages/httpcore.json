mment_text": "", "digests": {"md5": "4bac5a11a8df0d603fad40ae8078bc25", "sha256": "e3d5d110f49012dd3f1980ba5779f91170457cde43851e531e56e6eb08f05357"}, "downloads": -1, "filename": "httpcore-0.2.1.tar.gz", "has_sig": false, "md5_digest": "4bac5a11a8df0d603fad40ae8078bc25", "packagetype": "sdist", "python_version": "source", "requires_python": ">=3.6", "size": 12644, "upload_time": "2019-04-24T10:53:18", "upload_time_iso_8601": "2019-04-24T10:53:18.673960Z", "url": "https://files.pythonhosted.org/packages/0a/77/fad1ad195add3d50b8e67ee7e7eab2f80ba5cc9ae50347781472bb96c835/httpcore-0.2.1.tar.gz", "yanked": false}], "0.3.0": [{"comment_text": "", "digests": {"md5": "f9c68a3fbd3911f4c77adca448ef5f7d", "sha256": "96f910b528d47b683242ec207050c7bbaa99cd1b9a07f78ea80cf61e55556b58"}, "downloads": -1, "filename": "httpcore-0.3.0.tar.gz", "has_sig": false, "md5_digest": "f9c68a3fbd3911f4c77adca448ef5f7d", "packagetype": "sdist", "python_version": "source", "requires_python": ">=3.6", "size": 24847, "upload_time": "2019-05-16T11:43:03", "upload_time_iso_8601": "2019-05-16T11:43:03.655587Z", "url": "https://files.pythonhosted.org/packages/fd/6a/4447154dbbe7cac4cd390765caebe4b28febd53d86420e2ced334b3d4d23/httpcore-0.3.0.tar.gz", "yanked": false}], "0.4.0": [{"comment_text": "", "digests": {"md5": "d737d955173442b68f5e6dc00512455e", "sha256": "64a6bb0ed24d8535191d392d06dcd5071d363ce2c2b8f5db1062b503ba95f3b5"}, "downloads": -1, "filename": "httpcore-0.4.0.tar.gz", "has_sig": false, "md5_digest": "d737d955173442b68f5e6dc00512455e", "packagetype": "sdist", "python_version": "source", "requires_python": ">=3.6", "size": 29266, "upload_time": "2019-06-06T14:24:12", "upload_time_iso_8601": "2019-06-06T14:24:12.462787Z", "url": "https://files.pythonhosted.org/packages/43/96/909386e664eb5571e8091c86589f96aee8fcb77aa33ca07c9f49e0d41115/httpcore-0.4.0.tar.gz", "yanked": false}], "0.5.0": [{"comment_text": "", "digests": {"md5": "c96b045e3a28b4022590fc432b744478", "sha256": "b5ed6a4eb3d44cfbd3