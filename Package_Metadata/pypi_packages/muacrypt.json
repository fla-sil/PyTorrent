es_python": null, "size": 50950, "upload_time": "2018-02-22T14:14:45", "upload_time_iso_8601": "2018-02-22T14:14:45.734421Z", "url": "https://files.pythonhosted.org/packages/41/06/0f44fee5287805924345dfa50d06b20a4d9fba3ab5b765238fc191fda620/muacrypt-0.8.0.dev5.tar.gz", "yanked": false}], "0.8.0.dev6": [{"comment_text": "", "digests": {"md5": "18282a8262a4619eefab7e3dad69fd85", "sha256": "db230b2a7df8fd3e6a428f4357820b09ac1d88c77e0185db622c3f577cbb7625"}, "downloads": -1, "filename": "muacrypt-0.8.0.dev6-py2.py3-none-any.whl", "has_sig": false, "md5_digest": "18282a8262a4619eefab7e3dad69fd85", "packagetype": "bdist_wheel", "python_version": "2.7", "requires_python": null, "size": 31299, "upload_time": "2018-02-22T15:51:59", "upload_time_iso_8601": "2018-02-22T15:51:59.281838Z", "url": "https://files.pythonhosted.org/packages/e1/01/208ea5c1d0e625b2a31b98e9ae248be6f4580b179d349a3d14da5fdeaa0a/muacrypt-0.8.0.dev6-py2.py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "b63c585873b48a1b9638f070317113cb", "sha256": "a7878a1c2d0438524b29a59bd64309d277e7716f6dfeecbe2c68c0d22bae9e00"}, "downloads": -1, "filename": "muacrypt-0.8.0.dev6.tar.gz", "has_sig": false, "md5_digest": "b63c585873b48a1b9638f070317113cb", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 51030, "upload_time": "2018-02-22T15:52:01", "upload_time_iso_8601": "2018-02-22T15:52:01.026206Z", "url": "https://files.pythonhosted.org/packages/dd/91/fd27f4e788cf19fb42ac6f366ebe387d972a52699b7d8221ca0be31078f4/muacrypt-0.8.0.dev6.tar.gz", "yanked": false}], "0.8.1": [{"comment_text": "", "digests": {"md5": "23a86eca07c904c737837654e0e95a11", "sha256": "aa6c6e91ea35979f95ff7e370b10001a13a04630b2d74953214c32154e262743"}, "downloads": -1, "filename": "muacrypt-0.8.1-py2.py3-none-any.whl", "has_sig": false, "md5_digest": "23a86eca07c904c737837654e0e95a11", "packagetype": "bdist_wheel", "python_version": "2.7", "requires_python": null, "size": 54989, "upload_ti