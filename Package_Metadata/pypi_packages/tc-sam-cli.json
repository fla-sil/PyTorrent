"0.0.12": [{"comment_text": "", "digests": {"md5": "2e171fff09a7bf2a4842b0ccbe88b193", "sha256": "af9ecb4cd28614494c75150367353cec9030e68b86ca3dbcaa223afffec9184b"}, "downloads": -1, "filename": "tc_sam_cli-0.0.12-py3-none-any.whl", "has_sig": false, "md5_digest": "2e171fff09a7bf2a4842b0ccbe88b193", "packagetype": "bdist_wheel", "python_version": "py3", "requires_python": ">=3.7", "size": 6931, "upload_time": "2019-12-06T12:07:17", "upload_time_iso_8601": "2019-12-06T12:07:17.175393Z", "url": "https://files.pythonhosted.org/packages/99/0d/7baca1b7cfe7d585759c4d1670f712eedbf7172b3b9c67a5cca0596b2ef1/tc_sam_cli-0.0.12-py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "02cd393680e84d621af0d35cf8ebb308", "sha256": "e75fcc6a382b8c868bf959b05ad229fcb77188cc15a5db7035be9fa1654d3ce2"}, "downloads": -1, "filename": "tc-sam-cli-0.0.12.tar.gz", "has_sig": false, "md5_digest": "02cd393680e84d621af0d35cf8ebb308", "packagetype": "sdist", "python_version": "source", "requires_python": ">=3.7", "size": 9457, "upload_time": "2019-12-06T12:07:18", "upload_time_iso_8601": "2019-12-06T12:07:18.622677Z", "url": "https://files.pythonhosted.org/packages/94/8a/3f3d05552e8fed20a8947128cfeb7f974a391373a652f7c03ed8afac3f57/tc-sam-cli-0.0.12.tar.gz", "yanked": false}], "0.0.2": [{"comment_text": "", "digests": {"md5": "af687ddb39f5fc9c1100caecb55344af", "sha256": "e5075fabc1006f677df64e8c78c7b2c50297fb8816ea215951d3a06f2491bd7c"}, "downloads": -1, "filename": "tc_sam_cli-0.0.2-py3-none-any.whl", "has_sig": false, "md5_digest": "af687ddb39f5fc9c1100caecb55344af", "packagetype": "bdist_wheel", "python_version": "py3", "requires_python": ">=3.7", "size": 11099, "upload_time": "2019-04-30T18:12:18", "upload_time_iso_8601": "2019-04-30T18:12:18.778777Z", "url": "https://files.pythonhosted.org/packages/9a/83/066cbc78f47606909024ccdea177455005ef3555e833b3cefc6e39d51de3/tc_sam_cli-0.0.2-py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "dbc76980675