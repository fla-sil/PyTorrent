 {"comment_text": "", "digests": {"md5": "8bd918415d77ec019d30a303c50e2638", "sha256": "98b934e72c680e57b33f74867a5a78f28631844456d9a5722187862d3a807ce9"}, "downloads": -1, "filename": "cophi-1.0.5.tar.gz", "has_sig": false, "md5_digest": "8bd918415d77ec019d30a303c50e2638", "packagetype": "sdist", "python_version": "source", "requires_python": ">=3.4.0", "size": 11713, "upload_time": "2018-09-04T14:10:47", "upload_time_iso_8601": "2018-09-04T14:10:47.508360Z", "url": "https://files.pythonhosted.org/packages/fa/3e/693a73aafb8f14b9116ac38a5b640757fe640824c2b1224702db92b2a2d3/cophi-1.0.5.tar.gz", "yanked": false}], "1.0.6": [{"comment_text": "", "digests": {"md5": "f6a9615842267d8ad169ae0b733b5de6", "sha256": "0aae42f3eaf36431403da86b66a7d7a7fcf13d959e89baf0d59481c587ecd90a"}, "downloads": -1, "filename": "cophi-1.0.6-py3-none-any.whl", "has_sig": false, "md5_digest": "f6a9615842267d8ad169ae0b733b5de6", "packagetype": "bdist_wheel", "python_version": "py3", "requires_python": ">=3.4.0", "size": 12789, "upload_time": "2018-09-30T11:29:13", "upload_time_iso_8601": "2018-09-30T11:29:13.990317Z", "url": "https://files.pythonhosted.org/packages/af/75/bbb741c8a7330ba7c57bddb1071269b40f8333ec3e9a8eafaf22d416c174/cophi-1.0.6-py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "6a9df5be142909a0892d28bfda01697b", "sha256": "41a3259416d433268211ba84e3b325a0e180b18994383590f4a0299967d33b30"}, "downloads": -1, "filename": "cophi-1.0.6.tar.gz", "has_sig": false, "md5_digest": "6a9df5be142909a0892d28bfda01697b", "packagetype": "sdist", "python_version": "source", "requires_python": ">=3.4.0", "size": 12049, "upload_time": "2018-09-30T11:29:16", "upload_time_iso_8601": "2018-09-30T11:29:16.041205Z", "url": "https://files.pythonhosted.org/packages/99/86/7505f12adc6649b402a5960b4364cd15d740286e3178571fff45f38ba172/cophi-1.0.6.tar.gz", "yanked": false}], "1.0.7": [{"comment_text": "", "digests": {"md5": "34127ed7a4abeeee9e30e174c136a506", "sha256": "d583e55ffc223