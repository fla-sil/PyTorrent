t": "", "digests": {"md5": "864117a77600fda575dc2edc65456e89", "sha256": "c1db63cd613082f2d59bcaeb5232b1f9739b4f0c03dd90c6c4d0c6920ef01457"}, "downloads": -1, "filename": "quantum-blackbird-0.2.4.tar.gz", "has_sig": false, "md5_digest": "864117a77600fda575dc2edc65456e89", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 120363, "upload_time": "2020-04-17T14:09:48", "upload_time_iso_8601": "2020-04-17T14:09:48.780010Z", "url": "https://files.pythonhosted.org/packages/06/0f/0b903f546f0bdc7ea8c0749e7b1d69553c93729a8c7a51cd11fed7a1a649/quantum-blackbird-0.2.4.tar.gz", "yanked": false}]}, "urls": [{"comment_text": "", "digests": {"md5": "21c353c2e487e7c80bf638b0a9a7dae5", "sha256": "8fad321fcfb8f24aa965bb395a1141a5540a0928c9335a55d9194ccc7c381368"}, "downloads": -1, "filename": "quantum_blackbird-0.2.4-py3-none-any.whl", "has_sig": false, "md5_digest": "21c353c2e487e7c80bf638b0a9a7dae5", "packagetype": "bdist_wheel", "python_version": "py3", "requires_python": null, "size": 43798, "upload_time": "2020-04-17T14:09:46", "upload_time_iso_8601": "2020-04-17T14:09:46.841773Z", "url": "https://files.pythonhosted.org/packages/99/14/e192c8ccbfd22453156b8cd6b7dfa6dd555b6ba0b05af1fd7d75155a4efa/quantum_blackbird-0.2.4-py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "864117a77600fda575dc2edc65456e89", "sha256": "c1db63cd613082f2d59bcaeb5232b1f9739b4f0c03dd90c6c4d0c6920ef01457"}, "downloads": -1, "filename": "quantum-blackbird-0.2.4.tar.gz", "has_sig": false, "md5_digest": "864117a77600fda575dc2edc65456e89", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 120363, "upload_time": "2020-04-17T14:09:48", "upload_time_iso_8601": "2020-04-17T14:09:48.780010Z", "url": "https://files.pythonhosted.org/packages/06/0f/0b903f546f0bdc7ea8c0749e7b1d69553c93729a8c7a51cd11fed7a1a649/quantum-blackbird-0.2.4.tar.gz", "yanked": false}], "timestamp": "Fri May  8 03:08:51 2020"}