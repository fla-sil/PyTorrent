igests": {"md5": "126718c2e9ffaeb71096f414b2767850", "sha256": "d25b31bf0ffcce5de6c75003f928225d530de8251fdec893b68f267e83998382"}, "downloads": -1, "filename": "pyhelm-0.0.10.dev21.tar.gz", "has_sig": false, "md5_digest": "126718c2e9ffaeb71096f414b2767850", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 33689, "upload_time": "2019-04-03T13:55:12", "upload_time_iso_8601": "2019-04-03T13:55:12.659138Z", "url": "https://files.pythonhosted.org/packages/4a/9a/95c4ec841cf26215d0324268c2ae6348d7703af6943d4f4032cbf67af129/pyhelm-0.0.10.dev21.tar.gz", "yanked": false}], "0.0.11": [{"comment_text": "", "digests": {"md5": "e221399e34f64afdc49a530394d2299e", "sha256": "fbf2ea6ecb2e373ac06e75c5a2c1a9a9d4b7ae43aa65dc7ad9326f1b807980c1"}, "downloads": -1, "filename": "pyhelm-0.0.11-py2-none-any.whl", "has_sig": false, "md5_digest": "e221399e34f64afdc49a530394d2299e", "packagetype": "bdist_wheel", "python_version": "py2", "requires_python": null, "size": 45872, "upload_time": "2019-04-04T11:28:04", "upload_time_iso_8601": "2019-04-04T11:28:04.856345Z", "url": "https://files.pythonhosted.org/packages/55/7c/3799f5c3564d52cfa1cb21592747c7afb86b15b5f49c8e0f1f8d3deda18d/pyhelm-0.0.11-py2-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "0e773a066478996ffe66348cecb5072d", "sha256": "89efe96ae4b9c282c3f0f1f150734f62c47ff031ab79a917a3449d8b1341a62e"}, "downloads": -1, "filename": "pyhelm-0.0.11.tar.gz", "has_sig": false, "md5_digest": "0e773a066478996ffe66348cecb5072d", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 34049, "upload_time": "2019-04-04T11:28:06", "upload_time_iso_8601": "2019-04-04T11:28:06.451143Z", "url": "https://files.pythonhosted.org/packages/27/c1/75897a41debdcad09c9a9921da1e5820dc2ebda95a339a1e85a00ead94b4/pyhelm-0.0.11.tar.gz", "yanked": false}], "0.0.2": [{"comment_text": "", "digests": {"md5": "ab0de3d90052cda154fd122f6533e3dd", "sha256": "1c2980ec94468b9b3e58dbee0fb