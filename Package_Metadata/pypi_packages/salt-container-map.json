: "", "digests": {"md5": "559a758898c5ed7521723ae58d9a8936", "sha256": "51a2b99ab2d88a98e5e5c802783480fed2a344f31f256622d4cca80f1760bc95"}, "downloads": -1, "filename": "salt-container-map-0.1.12.tar.gz", "has_sig": false, "md5_digest": "559a758898c5ed7521723ae58d9a8936", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 21009, "upload_time": "2017-06-26T12:33:06", "upload_time_iso_8601": "2017-06-26T12:33:06.171029Z", "url": "https://files.pythonhosted.org/packages/33/5c/ae2e38ba9ffdd116e0eea81021774dbbed74c0984789277e561d08b236ba/salt-container-map-0.1.12.tar.gz", "yanked": false}], "0.1.2": [{"comment_text": "", "digests": {"md5": "c2cc582b5233705052c9a0141ea61f83", "sha256": "ba5a0e134cd4cb21bfc8907cf3364bcdd1dc936acfbe87989d747561856cb74d"}, "downloads": -1, "filename": "salt_container_map-0.1.2-py2.7.egg", "has_sig": false, "md5_digest": "c2cc582b5233705052c9a0141ea61f83", "packagetype": "bdist_egg", "python_version": "2.7", "requires_python": null, "size": 41261, "upload_time": "2015-09-03T20:07:13", "upload_time_iso_8601": "2015-09-03T20:07:13.284282Z", "url": "https://files.pythonhosted.org/packages/d0/c5/17c4b3ec3ffbae647eb83fdaac124b9374e2fbd17b26f6a1120d2b547111/salt_container_map-0.1.2-py2.7.egg", "yanked": false}, {"comment_text": "", "digests": {"md5": "c4925864d39006d0fa8c9bfafe9ffbd4", "sha256": "25904f9dc21a24d02cbc8b7d25f6578c0a123600838cc2b7f435eb30dc812b5c"}, "downloads": -1, "filename": "salt-container-map-0.1.2.tar.gz", "has_sig": false, "md5_digest": "c4925864d39006d0fa8c9bfafe9ffbd4", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 15614, "upload_time": "2015-09-03T20:07:07", "upload_time_iso_8601": "2015-09-03T20:07:07.572408Z", "url": "https://files.pythonhosted.org/packages/56/7f/cd4647f98b86b263ebff1c774c2a827a312e1a7fec2fb5134fcb6140a8c6/salt-container-map-0.1.2.tar.gz", "yanked": false}], "0.1.3": [{"comment_text": "", "digests": {"md5": "2e33f93f8b3482a977da5292a4