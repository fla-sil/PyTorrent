/files.pythonhosted.org/packages/60/ac/0613f40af4b69c2f181b46bc7c6a06ff44f974351d0fe8c43a326416c968/drf_aggregates-0.0.12-py2-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "6fc86706ce55eb2fa27b7ae014e96136", "sha256": "1a3be72caaee7b38764a69e982926d3fede3ec217c0500909e1d42c70b347672"}, "downloads": -1, "filename": "drf_aggregates-0.0.12-py3-none-any.whl", "has_sig": false, "md5_digest": "6fc86706ce55eb2fa27b7ae014e96136", "packagetype": "bdist_wheel", "python_version": "py3", "requires_python": null, "size": 5945, "upload_time": "2019-08-01T08:34:18", "upload_time_iso_8601": "2019-08-01T08:34:18.942355Z", "url": "https://files.pythonhosted.org/packages/49/26/598863043a9ca25b6fca49c7637aca3fed4abb54e458fc15b6800ef9690f/drf_aggregates-0.0.12-py3-none-any.whl", "yanked": false}], "timestamp": "Fri May  8 00:49:48 2020"}