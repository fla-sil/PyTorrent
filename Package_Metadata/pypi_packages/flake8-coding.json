 [{"comment_text": "", "digests": {"md5": "81a3038af7a6becaa8b6009fc4eb06c9", "sha256": "6d3502abd1803941cb257702f30264bf7bf0b46ce5af3b764207bc0dcdff8b46"}, "downloads": -1, "filename": "flake8_coding-1.1.1-py2.py3-none-any.whl", "has_sig": false, "md5_digest": "81a3038af7a6becaa8b6009fc4eb06c9", "packagetype": "bdist_wheel", "python_version": "2.7", "requires_python": null, "size": 3924, "upload_time": "2015-10-25T09:34:50", "upload_time_iso_8601": "2015-10-25T09:34:50.762394Z", "url": "https://files.pythonhosted.org/packages/17/5d/6b27110afdccceb02948d99e33a1b18f19ebf3e933021a2b9afa10032012/flake8_coding-1.1.1-py2.py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "4cc0eb17b3cddf3752cf8ea44b132555", "sha256": "cf83c7fd951f6dca47245959785c040522059826497b3530dab967b5772f422b"}, "downloads": -1, "filename": "flake8-coding-1.1.1.tar.gz", "has_sig": false, "md5_digest": "4cc0eb17b3cddf3752cf8ea44b132555", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 6391, "upload_time": "2015-10-25T09:34:46", "upload_time_iso_8601": "2015-10-25T09:34:46.706922Z", "url": "https://files.pythonhosted.org/packages/50/e2/5d3e5f9ee4bbaa627259b1b58afe716d4126d29ea589c7c91926b3142c9d/flake8-coding-1.1.1.tar.gz", "yanked": false}], "1.2.0": [{"comment_text": "", "digests": {"md5": "de2ec4bb9ea7842b9ae2b6fea3b083c7", "sha256": "cd4f41a922e65f92b0613dc130555c5edd0bb1787fd0a1eb9219866433cc70cf"}, "downloads": -1, "filename": "flake8-coding-1.2.0.tar.gz", "has_sig": false, "md5_digest": "de2ec4bb9ea7842b9ae2b6fea3b083c7", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 7272, "upload_time": "2016-05-19T16:31:15", "upload_time_iso_8601": "2016-05-19T16:31:15.253345Z", "url": "https://files.pythonhosted.org/packages/cf/a5/f5231922f5d3ec7c98379b9f1b1cb12c6570e7dddcef4114aa9a131c650b/flake8-coding-1.2.0.tar.gz", "yanked": false}], "1.2.1": [{"comment_text": "", "digests": {"md5": "2e666c4724bed54b0961