pe": "sdist", "python_version": "source", "requires_python": null, "size": 5130852, "upload_time": "2017-07-19T04:38:57", "upload_time_iso_8601": "2017-07-19T04:38:57.005666Z", "url": "https://files.pythonhosted.org/packages/d7/ec/e813fe45f59570902d0752c1ed15145565bbcea6cbe1867fd945bc567b2c/TADLib-0.3.1-dev1.tar.gz", "yanked": false}], "0.3.1-r1": [{"comment_text": "", "digests": {"md5": "4635b0c7bed9c96a1b2383286d2c4271", "sha256": "58439d1fac8e61accfc2a2332c2cb2d569e06b4ce149a2e4019af661640f6b50"}, "downloads": -1, "filename": "TADLib-0.3.1-r1.tar.gz", "has_sig": false, "md5_digest": "4635b0c7bed9c96a1b2383286d2c4271", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 5132984, "upload_time": "2017-08-18T11:32:05", "upload_time_iso_8601": "2017-08-18T11:32:05.062428Z", "url": "https://files.pythonhosted.org/packages/25/96/626c6d795f57d1b27f11c92cb51b81ec0f865538a2316234f6586dd8dbd3/TADLib-0.3.1-r1.tar.gz", "yanked": false}], "0.4.0": [{"comment_text": "", "digests": {"md5": "a04a459fce9083fedfa75b0609f7bac5", "sha256": "a13bffd8f48220c5c26e3cd1c5dda3e3747bc36f82d3a1003a625fbeb12c3675"}, "downloads": -1, "filename": "TADLib-0.4.0-py3-none-any.whl", "has_sig": false, "md5_digest": "a04a459fce9083fedfa75b0609f7bac5", "packagetype": "bdist_wheel", "python_version": "py3", "requires_python": null, "size": 64546, "upload_time": "2018-11-25T16:05:47", "upload_time_iso_8601": "2018-11-25T16:05:47.194629Z", "url": "https://files.pythonhosted.org/packages/f3/bd/dfd429cbe1aed70318b33177e56e804ac30b9ec0a117308ae8750606666d/TADLib-0.4.0-py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "8506d4c2b3821c4b72646593de63cea4", "sha256": "185f2434d8900235918c4b16daf4a53298a375529ae85845290f00034a8a5a32"}, "downloads": -1, "filename": "TADLib-0.4.0.tar.gz", "has_sig": false, "md5_digest": "8506d4c2b3821c4b72646593de63cea4", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 60685, "upload_ti