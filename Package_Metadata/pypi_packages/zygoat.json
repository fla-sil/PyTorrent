ests": {"md5": "8de235e52b70c7bd4c0c948d93fb88b9", "sha256": "411de976d994e6c949235208b4a7ff0676752fa55ab2d8bf8637c06cf4e01ddf"}, "downloads": -1, "filename": "zygoat-0.1.4-py3-none-any.whl", "has_sig": false, "md5_digest": "8de235e52b70c7bd4c0c948d93fb88b9", "packagetype": "bdist_wheel", "python_version": "py3", "requires_python": ">=3.7,<4.0", "size": 44547, "upload_time": "2020-03-16T22:58:33", "upload_time_iso_8601": "2020-03-16T22:58:33.518942Z", "url": "https://files.pythonhosted.org/packages/d1/4b/0973cb4891b28a05bb178600f9df26dd1168e0783e346cbc74ab41d6aab8/zygoat-0.1.4-py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "44b083660ba5d5253dd70e4bdaed1b11", "sha256": "fa5d8b26c8d0ce0520e1aa11a073b88de4bbdbeb4c1425a6be3ae029245755dd"}, "downloads": -1, "filename": "zygoat-0.1.4.tar.gz", "has_sig": false, "md5_digest": "44b083660ba5d5253dd70e4bdaed1b11", "packagetype": "sdist", "python_version": "source", "requires_python": ">=3.7,<4.0", "size": 23215, "upload_time": "2020-03-16T22:58:34", "upload_time_iso_8601": "2020-03-16T22:58:34.582493Z", "url": "https://files.pythonhosted.org/packages/2b/07/cff1ae92291ee027ab49600ea8704a6a51624a7007ca2a2ff40fb4b6a482/zygoat-0.1.4.tar.gz", "yanked": false}], "0.1.5": [{"comment_text": "", "digests": {"md5": "952bbebefd7862d48266e9fd5ba62242", "sha256": "8dbd302ad91d713d65763cc5908e4c64a6c228167f18aa387e4942bea88ee961"}, "downloads": -1, "filename": "zygoat-0.1.5-py3-none-any.whl", "has_sig": false, "md5_digest": "952bbebefd7862d48266e9fd5ba62242", "packagetype": "bdist_wheel", "python_version": "py3", "requires_python": ">=3.7,<4.0", "size": 44540, "upload_time": "2020-03-19T22:36:12", "upload_time_iso_8601": "2020-03-19T22:36:12.540329Z", "url": "https://files.pythonhosted.org/packages/b3/9e/fbd961ac9a75b97d47813aec5bc9d2607d437d2ffb7d272862a780f311a2/zygoat-0.1.5-py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "fd74770513c3f64b0e724f3947790d37", "sha256": "992945a8ca1f