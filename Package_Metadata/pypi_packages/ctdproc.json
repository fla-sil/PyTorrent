_url": "https://pypi.org/project/ctdproc/", "project_urls": {"Documentation": "https://ctdproc.readthedocs.io", "Homepage": "https://github.com/gunnarvoet/ctdproc"}, "release_url": "https://pypi.org/project/ctdproc/0.1.4/", "requires_dist": ["numpy", "xarray", "gsw", "scipy", "xmltodict", "pandas", "munch", "matplotlib", "pytest (>=3.8) ; extra == 'test'", "coverage (>=4.5) ; extra == 'test'", "pytest-cov (>=2.6) ; extra == 'test'", "tox (>=3.3) ; extra == 'test'", "codecov (>=2.0) ; extra == 'test'"], "requires_python": ">=3.6", "summary": "Library for CTD data processing", "version": "0.1.4", "yanked": false, "html_description": "<div class=\"project-description\">\n            <a href=\"https://pypi.python.org/pypi/ctdproc\" rel=\"nofollow\"><img alt=\"https://img.shields.io/pypi/v/ctdproc.svg\" src=\"https://warehouse-camo.ingress.cmh1.psfhosted.org/17d97eb8019304e83f69fc3fb2dd6ac1f176badb/68747470733a2f2f696d672e736869656c64732e696f2f707970692f762f63746470726f632e737667\"></a>\n<a href=\"https://travis-ci.com/gunnarvoet/ctdproc\" rel=\"nofollow\"><img alt=\"https://img.shields.io/travis/gunnarvoet/ctdproc.svg\" src=\"https://warehouse-camo.ingress.cmh1.psfhosted.org/32d103979586c5d4e3a0670d3f0036b3cd90b876/68747470733a2f2f696d672e736869656c64732e696f2f7472617669732f67756e6e6172766f65742f63746470726f632e737667\"></a>\n<a href=\"https://ctdproc.readthedocs.io/en/latest/?badge=latest\" rel=\"nofollow\"><img alt=\"Documentation Status\" src=\"https://warehouse-camo.ingress.cmh1.psfhosted.org/b7ca11680371c3c381b1ce4562f4d5ddc67fd884/68747470733a2f2f72656164746865646f63732e6f72672f70726f6a656374732f63746470726f632f62616467652f3f76657273696f6e3d6c6174657374\"></a>\n<a href=\"https://github.com/psf/black\" rel=\"nofollow\"><img alt=\"https://img.shields.io/badge/code%20style-black-000000.svg\" src=\"https://warehouse-camo.ingress.cmh1.psfhosted.org/fbfdc7754183ecf079bc71ddeabaf88f6cbc5c00/68747470733a2f2f696d672e736869656c64732e696f2f62616467652f636f64652532307374796c6