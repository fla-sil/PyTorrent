ked": false}], "1.0.1": [{"comment_text": "", "digests": {"md5": "5f36339e73a346599ee3121e7fc06953", "sha256": "9b5197d39eb42bec3d61c6ff614b27f3887fb816364df4ead07007a692a637d0"}, "downloads": -1, "filename": "python-lightblue-1.0.1.tar.gz", "has_sig": false, "md5_digest": "5f36339e73a346599ee3121e7fc06953", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 29286, "upload_time": "2020-04-22T12:31:22", "upload_time_iso_8601": "2020-04-22T12:31:22.701027Z", "url": "https://files.pythonhosted.org/packages/fe/60/4038c96e604dd67626df09540dd237b1405d60d36b334f34b22b0f2625e1/python-lightblue-1.0.1.tar.gz", "yanked": false}]}, "urls": [{"comment_text": "", "digests": {"md5": "5f36339e73a346599ee3121e7fc06953", "sha256": "9b5197d39eb42bec3d61c6ff614b27f3887fb816364df4ead07007a692a637d0"}, "downloads": -1, "filename": "python-lightblue-1.0.1.tar.gz", "has_sig": false, "md5_digest": "5f36339e73a346599ee3121e7fc06953", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 29286, "upload_time": "2020-04-22T12:31:22", "upload_time_iso_8601": "2020-04-22T12:31:22.701027Z", "url": "https://files.pythonhosted.org/packages/fe/60/4038c96e604dd67626df09540dd237b1405d60d36b334f34b22b0f2625e1/python-lightblue-1.0.1.tar.gz", "yanked": false}], "timestamp": "Fri May  8 03:16:46 2020"}