an class=\"n\">get</span><span class=\"p\">(</span><span class=\"s1\">'https://service-</span><span class=\"si\">{appid}</span><span class=\"s1\">.ap-shanghai.apigateway.myqcloud.com/release/api'</span><span class=\"p\">,</span> <span class=\"n\">headers</span><span class=\"o\">=</span><span class=\"n\">headers</span><span class=\"p\">)</span>\n<span class=\"nb\">print</span><span class=\"p\">(</span><span class=\"n\">response</span><span class=\"o\">.</span><span class=\"n\">json</span><span class=\"p\">())</span>\n</pre>\n</div>\n<div id=\"author\">\n<h2>Author</h2>\n<p>Yixian Du</p>\n</div>\n\n          </div>"}, "last_serial": 4123364, "releases": {"0.1.0": [{"comment_text": "", "digests": {"md5": "5a1350b42ca19c94e257b8f45b6ee110", "sha256": "6f039b3b0167f0b0d6354f45fac7b75864dfb335316aaea4eac33483f2e2b78e"}, "downloads": -1, "filename": "tencentcloud_apigateway_authorization-0.1.0-py2.py3-none-any.whl", "has_sig": false, "md5_digest": "5a1350b42ca19c94e257b8f45b6ee110", "packagetype": "bdist_wheel", "python_version": "py2.py3", "requires_python": null, "size": 2805, "upload_time": "2018-08-01T05:42:15", "upload_time_iso_8601": "2018-08-01T05:42:15.365350Z", "url": "https://files.pythonhosted.org/packages/c6/f2/22a02c08eca21211a46367ed2acd065cc67cf9dc2e261bc70b3f2b48e669/tencentcloud_apigateway_authorization-0.1.0-py2.py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "6015ce80c7811a1b2c901feebaaf8ebe", "sha256": "2e2556657eb4f2619f5beba0e11bc5c36681f6f47208f6b82e4af773f8c462fe"}, "downloads": -1, "filename": "tencentcloud-apigateway-authorization-0.1.0.tar.gz", "has_sig": false, "md5_digest": "6015ce80c7811a1b2c901feebaaf8ebe", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 3107, "upload_time": "2018-08-01T05:42:16", "upload_time_iso_8601": "2018-08-01T05:42:16.702171Z", "url": "https://files.pythonhosted.org/packages/57/d0/238b1a539619983c7c8a2dca35e6e7ed8a2730cfcc6074e2ce6b50388c72/tencentcloud-