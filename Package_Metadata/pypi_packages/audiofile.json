: false}], "0.1.3": [{"comment_text": "", "digests": {"md5": "fedd83ed75719bfb4f8f6642774b9d35", "sha256": "c5d409a2cf780550e9007450269442238e416e0795ae5f1eb90786b033823201"}, "downloads": -1, "filename": "audiofile-0.1.3-py3-none-any.whl", "has_sig": false, "md5_digest": "fedd83ed75719bfb4f8f6642774b9d35", "packagetype": "bdist_wheel", "python_version": "py3", "requires_python": null, "size": 6037, "upload_time": "2019-03-27T11:56:31", "upload_time_iso_8601": "2019-03-27T11:56:31.925023Z", "url": "https://files.pythonhosted.org/packages/4c/1a/aadd70e083d1258740a13e1bf6dcf95f9100db334cc0de832d9c1bfd2b06/audiofile-0.1.3-py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "3c471f70e9913ed5e0fb3bd063d74551", "sha256": "ebaaf796c8e6aa5218ab9b97ca1ddff231b3346999895d60f455c0a2a020425e"}, "downloads": -1, "filename": "audiofile-0.1.3.tar.gz", "has_sig": false, "md5_digest": "3c471f70e9913ed5e0fb3bd063d74551", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 10859, "upload_time": "2019-03-27T11:56:33", "upload_time_iso_8601": "2019-03-27T11:56:33.471998Z", "url": "https://files.pythonhosted.org/packages/43/0b/1c958dc1319742a8aa484c6ea7cbbdd02d160c0e994a9face2d14e87d08b/audiofile-0.1.3.tar.gz", "yanked": false}], "0.2.0": [{"comment_text": "", "digests": {"md5": "1c074e032f3c24ea3fd3d2db6dce6b48", "sha256": "af98915432bfe3ffc4a3eb04ee9a7b8a61cbb19d2a164cac0af5c2658962e83c"}, "downloads": -1, "filename": "audiofile-0.2.0-py3-none-any.whl", "has_sig": false, "md5_digest": "1c074e032f3c24ea3fd3d2db6dce6b48", "packagetype": "bdist_wheel", "python_version": "py3", "requires_python": null, "size": 3112, "upload_time": "2019-05-02T14:15:23", "upload_time_iso_8601": "2019-05-02T14:15:23.468246Z", "url": "https://files.pythonhosted.org/packages/fa/dc/942d4a67e12fc2c289252e4ed8ad698b449ca256281c35d7db6cfcb7975b/audiofile-0.2.0-py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "068f28d561205f0445ee