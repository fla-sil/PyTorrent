Some backends support incoming text messages:<br><br>    # urls.py<br><br>    from smsgateway import backends<br><br>    urlpatterns = ('',<br>        (r'^incoming_sms/$', backends.get_backend('mobileweb').handle_incoming),<br>    )<br><br>* Set up your SMS gateway to use this url for incoming messages.<br><br>\n          </div>"}, "last_serial": 5220731, "releases": {"2.1.10": [{"comment_text": "", "digests": {"md5": "d7b99c245c516e26f88fdf64b51c9656", "sha256": "3e198b43bceabde21352a09f198bec232358b4ab4fcbf147aa39d848c15c9575"}, "downloads": -1, "filename": "django_smsgateway-2.1.10-py2-none-any.whl", "has_sig": false, "md5_digest": "d7b99c245c516e26f88fdf64b51c9656", "packagetype": "bdist_wheel", "python_version": "2.7", "requires_python": null, "size": 138993, "upload_time": "2017-08-16T08:57:22", "upload_time_iso_8601": "2017-08-16T08:57:22.144236Z", "url": "https://files.pythonhosted.org/packages/11/9a/837dfbafab42c44dc1db0654db7846189c159427d23bbc4587a6d9b19e04/django_smsgateway-2.1.10-py2-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "e32986cda77677849558cbfde7734856", "sha256": "ef15b7f80fda10819f8dffc37fe9ea8c2f20d4428663ca6987a994c9ce3050b3"}, "downloads": -1, "filename": "django-smsgateway-2.1.10.tar.gz", "has_sig": false, "md5_digest": "e32986cda77677849558cbfde7734856", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 90663, "upload_time": "2017-08-16T08:57:35", "upload_time_iso_8601": "2017-08-16T08:57:35.767157Z", "url": "https://files.pythonhosted.org/packages/c9/e6/1f65e918a4090f55650974be801142c971819edfec011a1a1dbf0d84b030/django-smsgateway-2.1.10.tar.gz", "yanked": false}], "2.1.11": [{"comment_text": "", "digests": {"md5": "6a6d31fea25ca6c4e9d82e6450830c2c", "sha256": "af3aaf0704a55df50fbb98c5d37a8bdc15625c9c4f52687536d1be7ef4e5c5fa"}, "downloads": -1, "filename": "django_smsgateway-2.1.11-py2.py3-none-any.whl", "has_sig": false, "md5_digest": "6a6d31fea25ca6c4e9d82e6450830c2c", "pa