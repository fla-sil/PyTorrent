dist_wheel", "python_version": "py3", "requires_python": ">=3.4", "size": 11006, "upload_time": "2018-09-18T15:31:28", "upload_time_iso_8601": "2018-09-18T15:31:28.584404Z", "url": "https://files.pythonhosted.org/packages/78/c8/ad0f981fe2b2af0ffd1dbd5534019e62b1a28beb5e7749c61389b8531834/pybase16_builder-0.1.11-py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "f8ef365f1b1bba13814053146b9f4676", "sha256": "0e811d800447525767bcf1bbd73c965e8f04d85c3081becc81a0933114dbfa9d"}, "downloads": -1, "filename": "pybase16-builder-0.1.11.tar.gz", "has_sig": false, "md5_digest": "f8ef365f1b1bba13814053146b9f4676", "packagetype": "sdist", "python_version": "source", "requires_python": ">=3.4", "size": 10414, "upload_time": "2018-09-18T15:31:29", "upload_time_iso_8601": "2018-09-18T15:31:29.928532Z", "url": "https://files.pythonhosted.org/packages/e9/0d/0fae939f8592f310332e8053bf3299be79a300c6dee3ca9fe080e42da144/pybase16-builder-0.1.11.tar.gz", "yanked": false}], "0.1.12": [{"comment_text": "", "digests": {"md5": "a63605264c66df5221eb3c2d4e783f8d", "sha256": "46048cfa62b821c07d02f5bce878a590c195f10b36141b2b083314760daf2cd0"}, "downloads": -1, "filename": "pybase16_builder-0.1.12-py3-none-any.whl", "has_sig": false, "md5_digest": "a63605264c66df5221eb3c2d4e783f8d", "packagetype": "bdist_wheel", "python_version": "py3", "requires_python": ">=3.4", "size": 11543, "upload_time": "2019-06-03T12:57:05", "upload_time_iso_8601": "2019-06-03T12:57:05.022075Z", "url": "https://files.pythonhosted.org/packages/b5/ad/ef3fdf3b4e6b47e17b1020b265d52a0ea2ea329a08d48ef1eb1162dafd52/pybase16_builder-0.1.12-py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "c256d2cdad15b355a6f882c0cac3838a", "sha256": "915eef32828d262ca63b534919aa755bd2637fcb5a45b8e961d8ba87a21b0cb4"}, "downloads": -1, "filename": "pybase16-builder-0.1.12.tar.gz", "has_sig": false, "md5_digest": "c256d2cdad15b355a6f882c0cac3838a", "packagetype": "sdist", "python_version": "source