": [{"comment_text": "", "digests": {"md5": "e22e77a8af55955f9f9d7e9b6bbe45ca", "sha256": "3b1a2fad2bdc4217acd37567d29e784af303f0f7c7cd5bb85dc63adc2c99a5e8"}, "downloads": -1, "filename": "fiobank-1.2.0-py2.py3-none-any.whl", "has_sig": false, "md5_digest": "e22e77a8af55955f9f9d7e9b6bbe45ca", "packagetype": "bdist_wheel", "python_version": "py2.py3", "requires_python": null, "size": 7330, "upload_time": "2016-08-04T11:22:11", "upload_time_iso_8601": "2016-08-04T11:22:11.378375Z", "url": "https://files.pythonhosted.org/packages/ea/21/7f14d86ed6e9204058c9b7a524b6d4b726270ed92a6c50d6deeb35e8fd5f/fiobank-1.2.0-py2.py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "c716e665ec748f2b50b357da9aeaa0b1", "sha256": "4a89c02d0d2f44a87b92ee66fce0af878f2ea11604315e0f2a6fb91c169c5e20"}, "downloads": -1, "filename": "fiobank-1.2.0.tar.gz", "has_sig": false, "md5_digest": "c716e665ec748f2b50b357da9aeaa0b1", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 5148, "upload_time": "2016-08-04T11:22:09", "upload_time_iso_8601": "2016-08-04T11:22:09.177174Z", "url": "https://files.pythonhosted.org/packages/6f/8f/26d7197155a7b0ead2bd6e5f5a617f2543a33cab57db13a3632991c03e98/fiobank-1.2.0.tar.gz", "yanked": false}], "timestamp": "Fri May  8 00:42:18 2020"}