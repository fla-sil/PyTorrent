oad_time_iso_8601": "2020-03-23T14:52:57.142779Z", "url": "https://files.pythonhosted.org/packages/16/ef/1aafaf522880311cea035b50be20a207ffa1d051422272faa3365a009755/gaussianlda-0.1.6.tar.gz", "yanked": false}], "0.2.0": [{"comment_text": "", "digests": {"md5": "4eee79659db202417dbaa11c127da930", "sha256": "edc1b6b2e330f07bff4b85e92136b95c6767ae2886e5421d7c092c7c1fb08093"}, "downloads": -1, "filename": "gaussianlda-0.2.0-py3-none-any.whl", "has_sig": false, "md5_digest": "4eee79659db202417dbaa11c127da930", "packagetype": "bdist_wheel", "python_version": "py3", "requires_python": ">=3.2", "size": 37704, "upload_time": "2020-03-27T14:08:33", "upload_time_iso_8601": "2020-03-27T14:08:33.684966Z", "url": "https://files.pythonhosted.org/packages/d5/12/a836a88331f453a549789693188dfd270969b1db42d301a2c139abe628f3/gaussianlda-0.2.0-py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "e2854295d9326010e2af0be4b9aeb734", "sha256": "6806133eee884832beb46849dd2a50936e45b65cc75167eaacc14119fbc29791"}, "downloads": -1, "filename": "gaussianlda-0.2.0.tar.gz", "has_sig": false, "md5_digest": "e2854295d9326010e2af0be4b9aeb734", "packagetype": "sdist", "python_version": "source", "requires_python": ">=3.2", "size": 22076, "upload_time": "2020-03-27T14:08:34", "upload_time_iso_8601": "2020-03-27T14:08:34.911837Z", "url": "https://files.pythonhosted.org/packages/2c/9f/3a7cca41f4795b9f6e5119c988a82eb60602550a2fef9c4246c60683f671/gaussianlda-0.2.0.tar.gz", "yanked": false}], "0.2.1": [{"comment_text": "", "digests": {"md5": "89665009c0c49d888902daebfe3fdf01", "sha256": "fe53efd38f57a154f856160e5de86854b8baafe55f0506b9be5990f13a105d32"}, "downloads": -1, "filename": "gaussianlda-0.2.1-py3-none-any.whl", "has_sig": false, "md5_digest": "89665009c0c49d888902daebfe3fdf01", "packagetype": "bdist_wheel", "python_version": "py3", "requires_python": ">=3.2", "size": 41367, "upload_time": "2020-04-06T16:17:48", "upload_time_iso_8601": "2020-04-06T16:17:48.016836Z", "url": 