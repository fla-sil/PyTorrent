oads": -1, "filename": "lektor_google_analytics-0.1.3-py2-none-any.whl", "has_sig": false, "md5_digest": "2e855c08fad13a71ea764d4c4fdf945f", "packagetype": "bdist_wheel", "python_version": "2.7", "requires_python": null, "size": 5440, "upload_time": "2016-01-02T19:02:29", "upload_time_iso_8601": "2016-01-02T19:02:29.095281Z", "url": "https://files.pythonhosted.org/packages/a5/4f/9ed946e6539ab1874285b27698695ae1238e3438a298f445aa3d6989c2ac/lektor_google_analytics-0.1.3-py2-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "da46ef73655bdefb0895e63e3d74e549", "sha256": "3a527bc854371f4ab1aab971749824148f5d9bd18284eaa0ee0eaf7be34009fb"}, "downloads": -1, "filename": "lektor-google-analytics-0.1.3.tar.gz", "has_sig": false, "md5_digest": "da46ef73655bdefb0895e63e3d74e549", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 3729, "upload_time": "2016-01-02T19:02:18", "upload_time_iso_8601": "2016-01-02T19:02:18.613915Z", "url": "https://files.pythonhosted.org/packages/3a/a3/f3b3557a7a6e2fc6cb05d7e5ff73bf94d6f67a5ff7e32ec4471654d735e8/lektor-google-analytics-0.1.3.tar.gz", "yanked": false}], "timestamp": "Fri May  8 00:47:08 2020"}