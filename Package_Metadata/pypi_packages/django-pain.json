_url": "https://pypi.org/project/django-pain/", "platform": "", "project_url": "https://pypi.org/project/django-pain/", "project_urls": {"Homepage": "https://github.com/stinovlas/django-pain"}, "release_url": "https://pypi.org/project/django-pain/0.3.0/", "requires_dist": ["Django (>=2.0)", "django-app-settings", "django-modeltranslation (>=0.13-beta1)", "django-money (>=0.14.2)", "lxml", "flake8; extra == 'quality'", "isort; extra == 'quality'", "mypy; extra == 'quality'", "polint; extra == 'quality'", "pydocstyle; extra == 'quality'", "freezegun; extra == 'test'", "testfixtures; extra == 'test'"], "requires_python": "", "summary": "Django application for managing bank payments and invoices", "version": "0.3.0", "yanked": false, "html_description": "<div class=\"project-description\">\n            <a href=\"https://travis-ci.org/stinovlas/django-pain\" rel=\"nofollow\"><img alt=\"https://travis-ci.org/stinovlas/django-pain.svg?branch=master\" src=\"https://warehouse-camo.ingress.cmh1.psfhosted.org/bda217f7ae4ee0ea19a1d00fa2239e3678771354/68747470733a2f2f7472617669732d63692e6f72672f7374696e6f766c61732f646a616e676f2d7061696e2e7376673f6272616e63683d6d6173746572\"></a>\n<a href=\"https://codecov.io/gh/stinovlas/django-pain\" rel=\"nofollow\"><img alt=\"https://codecov.io/gh/stinovlas/django-pain/branch/master/graph/badge.svg\" src=\"https://warehouse-camo.ingress.cmh1.psfhosted.org/89f291438179b3ea28001e7e25feaeb4041fd11e/68747470733a2f2f636f6465636f762e696f2f67682f7374696e6f766c61732f646a616e676f2d7061696e2f6272616e63682f6d61737465722f67726170682f62616467652e737667\"></a>\n<a href=\"https://pypi.org/project/django-pain\" rel=\"nofollow\"><img alt=\"https://img.shields.io/pypi/pyversions/django-pain.svg\" src=\"https://warehouse-camo.ingress.cmh1.psfhosted.org/5b77651e32c6cdb90c6ae52179e9213934d9eb77/68747470733a2f2f696d672e736869656c64732e696f2f707970692f707976657273696f6e732f646a616e676f2d7061696e2e737667\"></a>\n<a href=\"https://pypi.org/project/django-pain\" rel=\