on": "cp37", "requires_python": null, "size": 2251662, "upload_time": "2019-09-26T08:09:18", "upload_time_iso_8601": "2019-09-26T08:09:18.731143Z", "url": "https://files.pythonhosted.org/packages/48/7b/88c713af9c85bad05bf4f8d9404bdc7f4f2f5054d29ff9486ca8831098b9/genicam-0.0.0-cp37-cp37m-macosx_10_14_x86_64.whl", "yanked": false}], "0.0.1": [{"comment_text": "", "digests": {"md5": "50d8cf9e4d5398c90cd18076eda1d0ca", "sha256": "3150ed103c609ef9b0359688638642ad7f78ce5f91d6863748a4bb0bc3417ff7"}, "downloads": -1, "filename": "genicam-0.0.1-cp34-cp34m-macosx_10_14_x86_64.whl", "has_sig": false, "md5_digest": "50d8cf9e4d5398c90cd18076eda1d0ca", "packagetype": "bdist_wheel", "python_version": "cp34", "requires_python": null, "size": 2251689, "upload_time": "2019-09-26T08:20:31", "upload_time_iso_8601": "2019-09-26T08:20:31.922816Z", "url": "https://files.pythonhosted.org/packages/1c/48/2f57a447edeac63e989b2b7202ea5ce3b54179dc0021a1142c86076cf4e6/genicam-0.0.1-cp34-cp34m-macosx_10_14_x86_64.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "d20fc55e115a849aa7b887ba522757df", "sha256": "98496a1ffbe3cf82034a228f686680fb86da7f3d4b094109a0c03fded37c0090"}, "downloads": -1, "filename": "genicam-0.0.1-cp35-cp35m-macosx_10_14_x86_64.whl", "has_sig": false, "md5_digest": "d20fc55e115a849aa7b887ba522757df", "packagetype": "bdist_wheel", "python_version": "cp35", "requires_python": null, "size": 2251682, "upload_time": "2019-09-26T08:20:37", "upload_time_iso_8601": "2019-09-26T08:20:37.379447Z", "url": "https://files.pythonhosted.org/packages/bd/7b/7a34a0ee183dbc9b51d9c55b4f8af7790fa09c0636b45849eaf49d11a8c9/genicam-0.0.1-cp35-cp35m-macosx_10_14_x86_64.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "f86c95c67d3f84685ce210b8ade8569f", "sha256": "e529c0c85e71df713f842cfdbba7129070ba75faaf3877e6bb79b22b1faec371"}, "downloads": -1, "filename": "genicam-0.0.1-cp36-cp36m-macosx_10_14_x86_64.whl", "has_sig": false, "md5_digest": "f86c95c67d3f84685ce210b8ade856