an><span class=\"p\">()</span>\n\n<span class=\"n\">shc</span><span class=\"o\">.</span><span class=\"n\">print_parameters</span><span class=\"p\">()</span>\n<span class=\"n\">shc</span><span class=\"o\">.</span><span class=\"n\">print_astro_object_classifications</span><span class=\"p\">()</span>\n<span class=\"n\">shc</span><span class=\"o\">.</span><span class=\"n\">print_astro_objects</span><span class=\"p\">()</span>\n\n<span class=\"nb\">print</span><span class=\"p\">(</span><span class=\"s2\">\"</span><span class=\"se\">\\n</span><span class=\"s2\">Retrieved </span><span class=\"si\">{0}</span><span class=\"s2\"> references\"</span><span class=\"o\">.</span><span class=\"n\">format</span><span class=\"p\">(</span><span class=\"nb\">len</span><span class=\"p\">(</span><span class=\"n\">shc</span><span class=\"o\">.</span><span class=\"n\">references</span><span class=\"p\">)))</span>\n<span class=\"nb\">print</span><span class=\"p\">(</span><span class=\"s2\">\"</span><span class=\"se\">\\n</span><span class=\"s2\">Reference 0</span><span class=\"se\">\\n</span><span class=\"s2\">  </span><span class=\"si\">{0:&lt;25s}{1}</span><span class=\"s2\">\"</span><span class=\"o\">.</span><span class=\"n\">format</span><span class=\"p\">(</span><span class=\"s2\">\"Key\"</span><span class=\"p\">,</span> <span class=\"s2\">\"Value\"</span><span class=\"p\">))</span>\n<span class=\"k\">for</span> <span class=\"n\">k</span><span class=\"p\">,</span> <span class=\"n\">v</span> <span class=\"ow\">in</span> <span class=\"n\">shc</span><span class=\"o\">.</span><span class=\"n\">references_json</span><span class=\"p\">[</span><span class=\"mi\">0</span><span class=\"p\">]</span><span class=\"o\">.</span><span class=\"n\">items</span><span class=\"p\">():</span>\n    <span class=\"nb\">print</span><span class=\"p\">(</span><span class=\"s2\">\"  </span><span class=\"si\">{0:&lt;25s}{1}</span><span class=\"s2\">\"</span><span class=\"o\">.</span><span class=\"n\">format</span><span class=\"p\">(</span><span class=\"n\">k</span><span class=\"p\">,</span> <span class=\"n\">v</span><span class=\"p\">))</span>\n\n<span class=\"nb\">print</span><span class=\"p\">(</span><span class=\"s2\">\"shc.observations.shape:</span><span class=\"se\">\\n</span><span class=\"si\">{}</span><span class=\"se\">\\n</span><span class=\"s2\">\"</span><span class=\"o\">.</span><span class=\"n\">format</span><span class=\"p\">(</span><span class=\"n\">shc</span><span class=\"o\">.</span><span class=\"n\">observations</span><span class=\"o\">.</span><span class=\"n\">shape</span><span class=\"p\">))</span>\n<span class=\"nb\">print</span><span class=\"p\">(</span><span class=\"s2\">\"shc.observations.dtype:</span><span class=\"se\">\\n</span><span class=\"si\">{}</span><span class=\"se\">\\n</span><span class=\"s2\">\"</span><span class=\"o\">.</span><span class=\"n\">format</span><span class=\"p\">(</span><span class=\"n\">shc</span><span class=\"o\">.</span><span class=\"n\">observations</span><span class=\"o\">.</span><span class=\"n\">dtype</span><span class=\"p\">))</span>\n<span class=\"nb\">print</span><span class=\"p\">(</span><span class=\"s2\">\"shc.observations['RA']:</span><span class=\"se\">\\n</span><span class=\"si\">{}</span><span class=\"se\">\\n</span><span class=\"s2\">\"</span><span class=\"o\">.</span><span class=\"n\">format</span><span class=\"p\">(</span><span class=\"n\">shc</span><span class=\"o\">.</span><span class=\"n\">observations</span><span class=\"p\">[</span><span class=\"s1\">'RA'</span><span class=\"p\">]))</span>\n<span class=\"nb\">print</span><span class=\"p\">(</span><span class=\"s2\">\"shc.observations_ref['RA']:</span><span class=\"se\">\\n</span><span class=\"si\">{}</span><span class=\"se\">\\n</span><span class=\"s2\">\"</span><span class=\"o\">.</span><span class=\"n\">format</span><span class=\"p\">(</span><span class=\"n\">shc</span><span class=\"o\">.</span><span class=\"n\">observations_ref</span><span class=\"p\