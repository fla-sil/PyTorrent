ls": [{"comment_text": "", "digests": {"md5": "4b01799fb18e7b1f333c0a4d711b7453", "sha256": "4a9d6c74b71877bfaca9c136bb432eb5932b386e6ff5a652a14add915e47ff58"}, "downloads": -1, "filename": "django2_bootstrap3_datetimepicker-3.0.2-py3-none-any.whl", "has_sig": false, "md5_digest": "4b01799fb18e7b1f333c0a4d711b7453", "packagetype": "bdist_wheel", "python_version": "3.6", "requires_python": null, "size": 124931, "upload_time": "2019-06-12T11:17:23", "upload_time_iso_8601": "2019-06-12T11:17:23.607886Z", "url": "https://files.pythonhosted.org/packages/1c/f0/1968961597913f3e02a1c0f2b0712918dca8cb3a24ca2c14056c8854732a/django2_bootstrap3_datetimepicker-3.0.2-py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "4d6a29c609079252f71821d55162c085", "sha256": "08445c8a4ee18bb0ff940b16bf814a1e0a8873e7d4f08c7b0190cc791efb6ff5"}, "downloads": -1, "filename": "django2-bootstrap3-datetimepicker-3.0.2.tar.gz", "has_sig": false, "md5_digest": "4d6a29c609079252f71821d55162c085", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 70087, "upload_time": "2019-06-12T11:17:20", "upload_time_iso_8601": "2019-06-12T11:17:20.796530Z", "url": "https://files.pythonhosted.org/packages/49/53/0bb3e6824088d38d2c765a9fd6a6fdf9c89bd55a79c0d74c96bc46602c30/django2-bootstrap3-datetimepicker-3.0.2.tar.gz", "yanked": false}], "timestamp": "Fri May  8 00:37:55 2020"}