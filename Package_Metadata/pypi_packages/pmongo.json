</span><span class=\"o\">.</span><span class=\"n\">count</span><span class=\"p\">())</span>\n\n<span class=\"c1\"># delete age field</span>\n<span class=\"n\">d1</span><span class=\"o\">.</span><span class=\"n\">unset</span><span class=\"p\">([</span><span class=\"s1\">'age'</span><span class=\"p\">])</span>\n<span class=\"nb\">print</span><span class=\"p\">(</span><span class=\"n\">d1</span><span class=\"p\">)</span>\n\n<span class=\"c1\"># update data</span>\n<span class=\"nb\">print</span><span class=\"p\">(</span><span class=\"s1\">'update'</span><span class=\"p\">,</span> <span class=\"n\">Data2</span><span class=\"o\">.</span><span class=\"n\">objects</span><span class=\"o\">.</span><span class=\"n\">find</span><span class=\"p\">(</span><span class=\"n\">age__between</span><span class=\"o\">=</span><span class=\"p\">(</span><span class=\"mi\">10</span><span class=\"p\">,</span> <span class=\"mi\">21</span><span class=\"p\">))</span><span class=\"o\">.</span><span class=\"n\">update</span><span class=\"p\">(</span><span class=\"n\">age</span><span class=\"o\">=</span><span class=\"mi\">25</span><span class=\"p\">))</span>\n\n<span class=\"nb\">print</span><span class=\"p\">(</span><span class=\"s1\">'between 10~21'</span><span class=\"p\">,</span> <span class=\"n\">Data2</span><span class=\"o\">.</span><span class=\"n\">objects</span><span class=\"o\">.</span><span class=\"n\">find</span><span class=\"p\">(</span><span class=\"n\">age__between</span><span class=\"o\">=</span><span class=\"p\">(</span><span class=\"mi\">10</span><span class=\"p\">,</span> <span class=\"mi\">21</span><span class=\"p\">))</span><span class=\"o\">.</span><span class=\"n\">count</span><span class=\"p\">())</span>\n\n<span class=\"nb\">print</span><span class=\"p\">(</span><span class=\"s1\">'age=25'</span><span class=\"p\">,</span> <span class=\"n\">Data2</span><span class=\"o\">.</span><span class=\"n\">objects</span><span class=\"o\">.</span><span class=\"n\">find</span><span clas