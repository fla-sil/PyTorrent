packages/70/2a/6321f6fde16ddca22a21eeb80b455a93df37127d589308ce0073d92c3775/darr-0.1.8.tar.gz", "yanked": false}], "0.1.9": [{"comment_text": "", "digests": {"md5": "0c6e0730b330ed19fc9bc014d23e3da9", "sha256": "e815db914f31f9001a2f4dacfb5543fea879513f240a4f8db0912e9e3e0d117c"}, "downloads": -1, "filename": "darr-0.1.9-py3-none-any.whl", "has_sig": false, "md5_digest": "0c6e0730b330ed19fc9bc014d23e3da9", "packagetype": "bdist_wheel", "python_version": "py3", "requires_python": null, "size": 53220, "upload_time": "2018-11-13T10:14:38", "upload_time_iso_8601": "2018-11-13T10:14:38.129493Z", "url": "https://files.pythonhosted.org/packages/b5/c2/6f55afe4d212137e2d759b413b3f73c7e29282cbf3e1dcd01a62c7b28fe2/darr-0.1.9-py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "03b81f4aeb0d595a9cfa4a47f64528e3", "sha256": "ab3a997043f7d3e466d2e2c3efe083889d854cd257914f73f5b9251cd9fc226f"}, "downloads": -1, "filename": "darr-0.1.9.tar.gz", "has_sig": false, "md5_digest": "03b81f4aeb0d595a9cfa4a47f64528e3", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 43821, "upload_time": "2018-11-13T10:14:40", "upload_time_iso_8601": "2018-11-13T10:14:40.458785Z", "url": "https://files.pythonhosted.org/packages/1c/66/e0e36c3b69dc58f884306e2ec9dea51a7720591d1f6a76302ea99eebf9af/darr-0.1.9.tar.gz", "yanked": false}], "0.2.0": [{"comment_text": "", "digests": {"md5": "a9e67e46be60181b7638376627dd5bd9", "sha256": "ed25707a0feb421db2ba2ad66fe71afbea0489060e6799578ce66bb5a5da4870"}, "downloads": -1, "filename": "darr-0.2.0-py3.6.egg", "has_sig": false, "md5_digest": "a9e67e46be60181b7638376627dd5bd9", "packagetype": "bdist_egg", "python_version": "3.6", "requires_python": ">=3.6", "size": 95696, "upload_time": "2020-05-07T09:23:21", "upload_time_iso_8601": "2020-05-07T09:23:21.320400Z", "url": "https://files.pythonhosted.org/packages/88/11/01c02a78a0f25abd70ba769644386e40edb823597a601faa2fbe2d6a46c8/darr-0.2.0-py3.6.egg", "yanked": false}