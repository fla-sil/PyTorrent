nv venv\nsource venv/bin/activate\npip install -r requirements.txt \n\n# build / test\npython3 setup.py install\npython3 -bb -m pytest tests\n\n# make dist\nls dist/\npython3 setup.py sdist\nls dist/\n\n# upload\npip install twine\ntwine upload dist/*\n</code></pre>\n\n          </div>"}, "last_serial": 6993823, "releases": {"0.1.8": [{"comment_text": "", "digests": {"md5": "60912b9c7fb024607c0620d8cfd0c059", "sha256": "36712a5f65c7daf72c5de1d45c1538aab45e6fdb742c3b7249904e8b5b6008a4"}, "downloads": -1, "filename": "amundsen_common_rudeserver-0.1.8-py3.7.egg", "has_sig": false, "md5_digest": "60912b9c7fb024607c0620d8cfd0c059", "packagetype": "bdist_egg", "python_version": "3.7", "requires_python": ">=3.6", "size": 19304, "upload_time": "2020-02-14T00:40:43", "upload_time_iso_8601": "2020-02-14T00:40:43.295764Z", "url": "https://files.pythonhosted.org/packages/86/d3/65361e7dac5ed7d856004ba455ba3476cdc3673aa5df131d8f03c5ec8b0c/amundsen_common_rudeserver-0.1.8-py3.7.egg", "yanked": false}, {"comment_text": "", "digests": {"md5": "96cadd9f2fbd406ccab6d251a211ff62", "sha256": "9eb333f4e7c0c97a4688060769f52b43b41b30dc26122c9d0de032046c7937c7"}, "downloads": -1, "filename": "amundsen-common-rudeserver-0.1.8.tar.gz", "has_sig": false, "md5_digest": "96cadd9f2fbd406ccab6d251a211ff62", "packagetype": "sdist", "python_version": "source", "requires_python": ">=3.6", "size": 6110, "upload_time": "2020-02-14T00:40:41", "upload_time_iso_8601": "2020-02-14T00:40:41.047303Z", "url": "https://files.pythonhosted.org/packages/d3/52/af93cee55886de42ba80f2befb4bb836c1ee211f21152bea6f4152d3452c/amundsen-common-rudeserver-0.1.8.tar.gz", "yanked": false}], "0.1.9": [{"comment_text": "", "digests": {"md5": "565614eef7a7c9265ce8fc6a2bb9538c", "sha256": "ffeb6ffb68266ee29f9d845ef7acade421fddbfd1f3161d55f56ba885813b15d"}, "downloads": -1, "filename": "amundsen_common_rudeserver-0.1.9-py3.7.egg", "has_sig": false, "md5_digest": "565614eef7a7c9265ce8fc6a2bb9538c", "packagetype": "bdist_egg", "pyt