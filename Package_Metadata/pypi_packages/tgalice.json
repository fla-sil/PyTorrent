 "digests": {"md5": "7de9e21f6ded116f6c1241a9a147c91e", "sha256": "197cef2b51d10e007f291a7f56523d810e8d4475043d6d1c5abea824cda520ad"}, "downloads": -1, "filename": "tgalice-0.0.7.tar.gz", "has_sig": false, "md5_digest": "7de9e21f6ded116f6c1241a9a147c91e", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 11051, "upload_time": "2019-06-25T06:59:50", "upload_time_iso_8601": "2019-06-25T06:59:50.087305Z", "url": "https://files.pythonhosted.org/packages/f4/ec/3dc8f7cdf25e55e894774530e79baf595aaef738c677882fd74732e97ba9/tgalice-0.0.7.tar.gz", "yanked": false}], "0.0.8": [{"comment_text": "", "digests": {"md5": "5b6d2847240f9fbce5132e716549402c", "sha256": "3401cff9908920e2ee1727b5e1f5879186e021258bed175f8b04d98377b918a5"}, "downloads": -1, "filename": "tgalice-0.0.8.tar.gz", "has_sig": false, "md5_digest": "5b6d2847240f9fbce5132e716549402c", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 11266, "upload_time": "2019-06-29T12:36:22", "upload_time_iso_8601": "2019-06-29T12:36:22.893337Z", "url": "https://files.pythonhosted.org/packages/2a/a2/b3b420830eb1e0afb1d6a9c192e12ce802bd4a107a4bdbf793f82c4ee20a/tgalice-0.0.8.tar.gz", "yanked": false}], "0.1.0": [{"comment_text": "", "digests": {"md5": "84480e5e80785ca8d7bfa34a1f4a1a75", "sha256": "39dc718123a4b7870691f0a52c2a07d5077462ff786012f0d8133d07b3ec4df6"}, "downloads": -1, "filename": "tgalice-0.1.0.tar.gz", "has_sig": false, "md5_digest": "84480e5e80785ca8d7bfa34a1f4a1a75", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 11525, "upload_time": "2019-07-31T22:14:18", "upload_time_iso_8601": "2019-07-31T22:14:18.830769Z", "url": "https://files.pythonhosted.org/packages/f5/32/42214705859f58545075937947f7d287ecfb7d6e182c48bd097fd16e9eae/tgalice-0.1.0.tar.gz", "yanked": false}], "0.1.1": [{"comment_text": "", "digests": {"md5": "3348dea2456387eacdbf4189df8d0673", "sha256": "0a65ced39d89d844127b8ecd7d8cfebd3cad0d13091e0d3da