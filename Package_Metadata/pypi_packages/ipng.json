span><span class=\"n\">png</span><span class=\"o\">.</span><span class=\"n\">width</span><span class=\"si\">}</span><span class=\"s1\">x</span><span class=\"si\">{</span><span class=\"n\">png</span><span class=\"o\">.</span><span class=\"n\">height</span><span class=\"si\">}</span><span class=\"s1\"> picture =)'</span><span class=\"p\">)</span>\n<span class=\"n\">png</span><span class=\"o\">.</span><span class=\"n\">render</span><span class=\"p\">(</span><span class=\"n\">output</span><span class=\"o\">=</span><span class=\"s1\">'path/to/output.png'</span><span class=\"p\">)</span>\n</pre>\n\n          </div>"}, "last_serial": 5230545, "releases": {"0.0.1": [{"comment_text": "", "digests": {"md5": "d425bc3573864e96ebe5463e841e4264", "sha256": "15ff28e2471b14dd1b08b749d4067cb525bf73f3a2ad6210ab009699e2cf2b46"}, "downloads": -1, "filename": "ipng-0.0.1-py3-none-any.whl", "has_sig": false, "md5_digest": "d425bc3573864e96ebe5463e841e4264", "packagetype": "bdist_wheel", "python_version": "py3", "requires_python": null, "size": 1932, "upload_time": "2019-05-03T04:29:56", "upload_time_iso_8601": "2019-05-03T04:29:56.638793Z", "url": "https://files.pythonhosted.org/packages/1c/8d/32defb8d14204ce4b46767e876e95fa8e0536b31b331479665faed769e1f/ipng-0.0.1-py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "1e93f024a6a4599861c62ee4c44f829d", "sha256": "b735a64585ecd0450fe29e45e21119a3264200c47e9b347c8e9e56a0b83946d4"}, "downloads": -1, "filename": "ipng-0.0.1.tar.gz", "has_sig": false, "md5_digest": "1e93f024a6a4599861c62ee4c44f829d", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 942, "upload_time": "2019-05-03T04:29:58", "upload_time_iso_8601": "2019-05-03T04:29:58.784464Z", "url": "https://files.pythonhosted.org/packages/5f/8c/0ba714ed81754d04340a2fe855859e25269d43f65d395c9b3e56de14388f/ipng-0.0.1.tar.gz", "yanked": false}], "0.0.2": [{"comment_text": "", "digests": {"md5": "7c3229a5c61492e346c961bbe2144f42", "sha