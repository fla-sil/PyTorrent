": false}, {"comment_text": "", "digests": {"md5": "790211eab61d5c11c1abc18b483e7a9b", "sha256": "0cfb38191308a0ac19149bbcc99aad45acb0d676b65bf1d6f888125e1cfdc8bf"}, "downloads": -1, "filename": "autoBWF-3.5.2.tar.gz", "has_sig": false, "md5_digest": "790211eab61d5c11c1abc18b483e7a9b", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 131616, "upload_time": "2020-04-16T00:13:46", "upload_time_iso_8601": "2020-04-16T00:13:46.888080Z", "url": "https://files.pythonhosted.org/packages/d5/59/288aeb7c9160f4bb0e88aabc82c10f9919e18bc8b83766f554103b165f59/autoBWF-3.5.2.tar.gz", "yanked": false}]}, "urls": [{"comment_text": "", "digests": {"md5": "ed3f4a06666b562d2aebece38d2f0249", "sha256": "2cbb06e643bcb9a1b02a7c1eed40d6fb6f381e3ea6638514259741d8aecac701"}, "downloads": -1, "filename": "autoBWF-3.5.2-py2.py3-none-any.whl", "has_sig": false, "md5_digest": "ed3f4a06666b562d2aebece38d2f0249", "packagetype": "bdist_wheel", "python_version": "py2.py3", "requires_python": null, "size": 40017, "upload_time": "2020-04-16T00:13:41", "upload_time_iso_8601": "2020-04-16T00:13:41.472567Z", "url": "https://files.pythonhosted.org/packages/03/da/4024d298536753c9629ffbef4d19c9fcd20b28dfa9056ff0e6b43c9b69e8/autoBWF-3.5.2-py2.py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "790211eab61d5c11c1abc18b483e7a9b", "sha256": "0cfb38191308a0ac19149bbcc99aad45acb0d676b65bf1d6f888125e1cfdc8bf"}, "downloads": -1, "filename": "autoBWF-3.5.2.tar.gz", "has_sig": false, "md5_digest": "790211eab61d5c11c1abc18b483e7a9b", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 131616, "upload_time": "2020-04-16T00:13:46", "upload_time_iso_8601": "2020-04-16T00:13:46.888080Z", "url": "https://files.pythonhosted.org/packages/d5/59/288aeb7c9160f4bb0e88aabc82c10f9919e18bc8b83766f554103b165f59/autoBWF-3.5.2.tar.gz", "yanked": false}], "timestamp": "Thu May  7 18:16:20 2020"}