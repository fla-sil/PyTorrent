unity.org.", "description_content_type": null, "docs_url": null, "download_url": "", "downloads": {"last_day": -1, "last_month": -1, "last_week": -1}, "home_page": "http://ursainfosystems.com", "keywords": "", "license": "AGPL-3", "maintainer": "", "maintainer_email": "", "name": "odoo9-addon-partner-contact-weight", "package_url": "https://pypi.org/project/odoo9-addon-partner-contact-weight/", "platform": "UNKNOWN", "project_url": "https://pypi.org/project/odoo9-addon-partner-contact-weight/", "project_urls": {"Homepage": "http://ursainfosystems.com"}, "release_url": "https://pypi.org/project/odoo9-addon-partner-contact-weight/9.0.1.0.1/", "requires_dist": ["odoo (<9.1a,>=9.0a)", "odoo9-addon-partner-contact-personal-information-page"], "requires_python": "", "summary": "Provide contact weight", "version": "9.0.1.0.1", "yanked": false, "html_description": "<div class=\"project-description\">\n            <a href=\"http://www.gnu.org/licenses/agpl-3.0-standalone.html\" rel=\"nofollow\"><img alt=\"License: AGPL-3\" src=\"https://warehouse-camo.ingress.cmh1.psfhosted.org/e275d7ebc120b0c2ff730ee763f7471f965e9298/68747470733a2f2f696d672e736869656c64732e696f2f62616467652f6c6963656e63652d4147504c2d2d332d626c75652e737667\"></a>\n<div id=\"partner-contact-weight\">\n<h2>Partner Contact Weight</h2>\n<p>This module adds the weight to a contact.</p>\n<div id=\"usage\">\n<h3>Usage</h3>\n<p>To use this module, you need to:</p>\n<ol>\n<li>Go to Contacts</li>\n<li>Create or select a contact</li>\n<li>In the Personal Information tab, enter its weight and the unit of measure</li>\n</ol>\n<a href=\"https://runbot.odoo-community.org/runbot/134/9.0\" rel=\"nofollow\"><img alt=\"Try me on Runbot\" src=\"https://warehouse-camo.ingress.cmh1.psfhosted.org/dbdbc606b84d2a8e7702f02bd61c256d7bad12f0/68747470733a2f2f6f646f6f2d636f6d6d756e6974792e6f72672f776562736974652f696d6167652f69722e6174746163686d656e742f353738345f663238313362642f6461746173\"></a>\n</div>\n<div id=\"known-issues-roadmap\">\