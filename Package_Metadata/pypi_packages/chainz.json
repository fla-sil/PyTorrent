 false}], "0.7": [{"comment_text": "", "digests": {"md5": "7e75229568277ccc2079617147dec1e1", "sha256": "b3b70da381e5c060d29c2077e4cb776307983872de18445f55ba694a3f7d78a4"}, "downloads": -1, "filename": "chainz-0.7.tar.gz", "has_sig": false, "md5_digest": "7e75229568277ccc2079617147dec1e1", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 5658, "upload_time": "2016-01-21T11:32:04", "upload_time_iso_8601": "2016-01-21T11:32:04.689251Z", "url": "https://files.pythonhosted.org/packages/6f/2d/212cf2fde347a009f1c1b663fcd8ab10ab5cfc3eb70620196a5964a74558/chainz-0.7.tar.gz", "yanked": false}], "0.8": [{"comment_text": "", "digests": {"md5": "1b71a8393f8f2eeaf9ba7e2f13f11311", "sha256": "e0fd6416e062670fd3008966ec8abe0a1ba4aef3207a906bb6c4edb1e827ece7"}, "downloads": -1, "filename": "chainz-0.8.tar.gz", "has_sig": false, "md5_digest": "1b71a8393f8f2eeaf9ba7e2f13f11311", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 6808, "upload_time": "2016-02-01T07:59:11", "upload_time_iso_8601": "2016-02-01T07:59:11.243000Z", "url": "https://files.pythonhosted.org/packages/83/aa/afcabb999cc04d66cdb91499a1c1bd9cec50e5ef9842439ccd24dc086f1e/chainz-0.8.tar.gz", "yanked": false}], "0.9": [{"comment_text": "", "digests": {"md5": "fb90ccc9e863b529d8fb846960001139", "sha256": "dece67d21f2190ee4bd19a868b838503cec65aeb808f92c46a341b84f36e513a"}, "downloads": -1, "filename": "chainz-0.9.tar.gz", "has_sig": false, "md5_digest": "fb90ccc9e863b529d8fb846960001139", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 6859, "upload_time": "2016-02-18T17:15:43", "upload_time_iso_8601": "2016-02-18T17:15:43.086290Z", "url": "https://files.pythonhosted.org/packages/89/5d/194209c7a54f7b4c0f198082efc1363e7e915c342ae66b5d06c621123bdc/chainz-0.9.tar.gz", "yanked": false}]}, "urls": [{"comment_text": "", "digests": {"md5": "031f087d075b48732627e444ede2c682", "sha256": "7ba5aa7d880ca3eca01d594159e439e953b2