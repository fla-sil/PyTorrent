, "description_content_type": "", "docs_url": null, "download_url": "", "downloads": {"last_day": -1, "last_month": -1, "last_week": -1}, "home_page": "http://www.savoirfairelinux.com", "keywords": "", "license": "AGPL-3", "maintainer": "", "maintainer_email": "", "name": "odoo9-addon-mgmtsystem-probability", "package_url": "https://pypi.org/project/odoo9-addon-mgmtsystem-probability/", "platform": "", "project_url": "https://pypi.org/project/odoo9-addon-mgmtsystem-probability/", "project_urls": {"Homepage": "http://www.savoirfairelinux.com"}, "release_url": "https://pypi.org/project/odoo9-addon-mgmtsystem-probability/9.0.1.0.0.99.dev14/", "requires_dist": ["odoo9-addon-mgmtsystem", "odoo (<9.1a,>=9.0a)"], "requires_python": "~=2.7", "summary": "Management System Probability", "version": "9.0.1.0.0.99.dev14", "yanked": false, "html_description": "<div class=\"project-description\">\n            <a href=\"http://www.gnu.org/licenses/agpl-3.0-standalone.html\" rel=\"nofollow\"><img alt=\"License: AGPL-3\" src=\"https://warehouse-camo.ingress.cmh1.psfhosted.org/e275d7ebc120b0c2ff730ee763f7471f965e9298/68747470733a2f2f696d672e736869656c64732e696f2f62616467652f6c6963656e63652d4147504c2d2d332d626c75652e737667\"></a>\n<div id=\"management-system-probability\">\n<h2>Management System Probability</h2>\n<p>This module allow you to manage scale of probability (or likelihood)\nused across different modules (health and safety, information security).</p>\n<div id=\"installation\">\n<h3>Installation</h3>\n<ul>\n<li>No specific installation required.</li>\n</ul>\n</div>\n<div id=\"configuration\">\n<h3>Configuration</h3>\n<ul>\n<li>No specific configuration required.</li>\n</ul>\n</div>\n<div id=\"usage\">\n<h3>Usage</h3>\n<p>To use this module, you need to:</p>\n<ul>\n<li>go to Management System &gt; Configuration &gt; Probability</li>\n</ul>\n<a href=\"https://runbot.odoo-community.org/runbot/128/9.0\" rel=\"nofollow\"><img alt=\"Try me on Runbot\" src=\"https://warehouse-camo.in