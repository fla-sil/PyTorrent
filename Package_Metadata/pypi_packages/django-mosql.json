: [{"comment_text": "", "digests": {"md5": "0d7d66339d72fe83dcbd51d058e33442", "sha256": "7557322268d3cbe92028593fadd2fabaa7fb201dcf9eb907ef79ad065ca57073"}, "downloads": -1, "filename": "django_mosql-0.4-py27-none-any.whl", "has_sig": false, "md5_digest": "0d7d66339d72fe83dcbd51d058e33442", "packagetype": "bdist_wheel", "python_version": "2.7", "requires_python": null, "size": 10594, "upload_time": "2013-12-18T07:19:27", "upload_time_iso_8601": "2013-12-18T07:19:27.088767Z", "url": "https://files.pythonhosted.org/packages/91/8d/16d56c8ceeeaee85f81544af8f6d066bcef3c95c92630cd146e07b9b0fa7/django_mosql-0.4-py27-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "6ca9e868f9de507f7f7a8355acd51696", "sha256": "678a3ebe7d3f83dfb30a17d7f47d1d3d98be3b583e96b763622bbd067cf14ec8"}, "downloads": -1, "filename": "django-mosql-0.4.tar.gz", "has_sig": false, "md5_digest": "6ca9e868f9de507f7f7a8355acd51696", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 8142, "upload_time": "2013-12-18T07:19:24", "upload_time_iso_8601": "2013-12-18T07:19:24.770218Z", "url": "https://files.pythonhosted.org/packages/3e/c6/a313bbcd47a40fe6a87aeb8991f54fed76bb0fe77893dbeb2e94c56b8895/django-mosql-0.4.tar.gz", "yanked": false}], "0.4.1": [{"comment_text": "", "digests": {"md5": "e3be4d832e3c4ccea80f8624bc391315", "sha256": "5b845d67b95e1f52e812f17e954f1513d3878d8c59f35c41e47b94844b37345c"}, "downloads": -1, "filename": "django_mosql-0.4.1-py27-none-any.whl", "has_sig": false, "md5_digest": "e3be4d832e3c4ccea80f8624bc391315", "packagetype": "bdist_wheel", "python_version": "2.7", "requires_python": null, "size": 13830, "upload_time": "2013-12-26T05:59:29", "upload_time_iso_8601": "2013-12-26T05:59:29.653038Z", "url": "https://files.pythonhosted.org/packages/d9/eb/12589bf4d3c26fcc18b65b69a9e257a5230c8d5d82820578f85b850f6496/django_mosql-0.4.1-py27-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "4a1f19084eb6fd8a2ee1cf6