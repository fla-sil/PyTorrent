ires_python": ">=2.7", "size": 29008, "upload_time": "2019-01-30T05:49:03", "upload_time_iso_8601": "2019-01-30T05:49:03.412319Z", "url": "https://files.pythonhosted.org/packages/3f/f5/45af693c929db5e565d9fe8b09a3d7f5cc3d91918213e1ad7fe45d84a00d/haka-mqtt-0.3.3.tar.gz", "yanked": false}], "0.3.4": [{"comment_text": "", "digests": {"md5": "541ba90da10b196793468c1b4d22becf", "sha256": "107f987d14d3fbce869ba499479241b3b7515a38df0471551b2ccd5f4cf18ad7"}, "downloads": -1, "filename": "haka-mqtt-0.3.4.tar.gz", "has_sig": true, "md5_digest": "541ba90da10b196793468c1b4d22becf", "packagetype": "sdist", "python_version": "source", "requires_python": ">=2.7", "size": 29132, "upload_time": "2019-02-01T03:41:28", "upload_time_iso_8601": "2019-02-01T03:41:28.359018Z", "url": "https://files.pythonhosted.org/packages/e3/c7/7b6b8b8f98303707c0e717633628d754739ada2eba4ab8d75d8a3c0797b7/haka-mqtt-0.3.4.tar.gz", "yanked": false}], "0.3.5": [{"comment_text": "", "digests": {"md5": "7a252a28d69a50e7c142a2364b539786", "sha256": "10022a78b3eea83d21c078f463ccad13c29b9cfb7adc7302f3f9e76b5675d498"}, "downloads": -1, "filename": "haka-mqtt-0.3.5.tar.gz", "has_sig": true, "md5_digest": "7a252a28d69a50e7c142a2364b539786", "packagetype": "sdist", "python_version": "source", "requires_python": ">=2.7", "size": 29485, "upload_time": "2019-02-13T06:30:24", "upload_time_iso_8601": "2019-02-13T06:30:24.892196Z", "url": "https://files.pythonhosted.org/packages/f6/58/f343dea6a9b85912b857af60164f78b940bb2b59a2de4c393f4a21e3700d/haka-mqtt-0.3.5.tar.gz", "yanked": false}]}, "urls": [{"comment_text": "", "digests": {"md5": "7a252a28d69a50e7c142a2364b539786", "sha256": "10022a78b3eea83d21c078f463ccad13c29b9cfb7adc7302f3f9e76b5675d498"}, "downloads": -1, "filename": "haka-mqtt-0.3.5.tar.gz", "has_sig": true, "md5_digest": "7a252a28d69a50e7c142a2364b539786", "packagetype": "sdist", "python_version": "source", "requires_python": ">=2.7", "size": 29485, "upload_time": "2019-02-13T06:30:24", "upload_time_iso_8601"