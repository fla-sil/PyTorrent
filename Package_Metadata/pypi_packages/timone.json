ts": {"md5": "ff251e3a467ca52c0b8e73ff5b400952", "sha256": "46fa99cb16348ba3a7de51644d881d7411911ac48909ddd3b7fee97210366b7a"}, "downloads": -1, "filename": "timone-0.3.0.tar.gz", "has_sig": false, "md5_digest": "ff251e3a467ca52c0b8e73ff5b400952", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 8717, "upload_time": "2020-01-10T15:13:05", "upload_time_iso_8601": "2020-01-10T15:13:05.193741Z", "url": "https://files.pythonhosted.org/packages/76/b5/9ba4f67dbde751784484c1804fa307d406361bb4a3c5d301c835f802bff5/timone-0.3.0.tar.gz", "yanked": false}], "0.3.1": [{"comment_text": "", "digests": {"md5": "b6cd132dd411f5da878ac40d6f1f63cd", "sha256": "f64a4aa40f8023d2e2cc3c0784d7c27b42786b96a65f8c52df29c04620fa2f3f"}, "downloads": -1, "filename": "timone-0.3.1-py3-none-any.whl", "has_sig": false, "md5_digest": "b6cd132dd411f5da878ac40d6f1f63cd", "packagetype": "bdist_wheel", "python_version": "py3", "requires_python": null, "size": 9621, "upload_time": "2020-01-10T15:38:30", "upload_time_iso_8601": "2020-01-10T15:38:30.825464Z", "url": "https://files.pythonhosted.org/packages/f3/3e/21e71d8848ad1925d2aa3a2fb1944f75b2ace6b14c41b871c6a175d8b28f/timone-0.3.1-py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "e37ee9a6d99299f6a177d90cfcfdc198", "sha256": "43d38941908be4fa45c6b31054e850ed6f7a66c7dbdd88d390aaefdfee52b385"}, "downloads": -1, "filename": "timone-0.3.1.tar.gz", "has_sig": false, "md5_digest": "e37ee9a6d99299f6a177d90cfcfdc198", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 8734, "upload_time": "2020-01-10T15:38:31", "upload_time_iso_8601": "2020-01-10T15:38:31.798266Z", "url": "https://files.pythonhosted.org/packages/4b/42/e8f125b8f883ebb2ea407e1b6abefe547cf54fc5a312909b85083744a77d/timone-0.3.1.tar.gz", "yanked": false}], "0.3.2": [{"comment_text": "", "digests": {"md5": "e764295f769139cb118aac5bdffcd142", "sha256": "a0e327a9ca373f6f8a8b79d994c4ba800431eefdfb9a8cb670d7f