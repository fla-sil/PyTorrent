=\"o\">=</span> <span class=\"s1\">''</span>\n</pre>\n</div>\n<div id=\"usage\">\n<h2>Usage</h2>\n<pre><span class=\"kn\">from</span> <span class=\"nn\">django</span> <span class=\"kn\">import</span> <span class=\"n\">forms</span>\n<span class=\"kn\">from</span> <span class=\"nn\">dropboxchooser_field.fields</span> <span class=\"kn\">import</span> <span class=\"n\">DropboxChooserField</span>\n\n<span class=\"k\">class</span> <span class=\"nc\">MyForm</span><span class=\"p\">(</span><span class=\"n\">forms</span><span class=\"o\">.</span><span class=\"n\">Form</span><span class=\"p\">):</span>\n    <span class=\"n\">file_from_dropbox</span> <span class=\"o\">=</span> <span class=\"n\">DropboxChooserField</span><span class=\"p\">(</span><span class=\"n\">extensions</span><span class=\"o\">=</span><span class=\"p\">[</span><span class=\"s1\">'jpg'</span><span class=\"p\">,</span> <span class=\"s1\">'png'</span><span class=\"p\">])</span>\n</pre>\n</div>\n\n          </div>"}, "last_serial": 1108437, "releases": {"0.0.1": [{"comment_text": "", "digests": {"md5": "2fe8b264ed952e3f6e375351c7877f55", "sha256": "6d402701ba0fae9aeca508f8baede6779932e312836522931e5a5270275c6084"}, "downloads": -1, "filename": "django-dropboxchooser-field-0.0.1.tar.gz", "has_sig": false, "md5_digest": "2fe8b264ed952e3f6e375351c7877f55", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 1801, "upload_time": "2013-10-21T19:01:47", "upload_time_iso_8601": "2013-10-21T19:01:47.681459Z", "url": "https://files.pythonhosted.org/packages/93/89/a764d15990eae346b162ca53565996963c76ea4855532a9edb5a6bf0f722/django-dropboxchooser-field-0.0.1.tar.gz", "yanked": false}], "0.0.2": [{"comment_text": "", "digests": {"md5": "2adcd75dc11fe938b63be6ab01790f2b", "sha256": "9272720da2fde0c158fea774a9d1e4a7142c089ee5193f53f87963e698d74c82"}, "downloads": -1, "filename": "django-dropboxchooser-field-0.0.2.tar.gz", "has_sig": false, "md5_digest": "2adcd75dc11fe938b63be6ab01790f2b", "p