_python": null, "size": 96740, "upload_time": "2007-05-22T21:36:11", "upload_time_iso_8601": "2007-05-22T21:36:11Z", "url": "https://files.pythonhosted.org/packages/b3/7a/daf59e8c32f07a3542a4a64dc36a33495faae934b9d55f9905d3d5e1cddf/z3c.formdemo-1.0.0c2-py2.4.egg", "yanked": false}, {"comment_text": "", "digests": {"md5": "75d9a26ea296dffad7e25cef9c332c7f", "sha256": "dfab2528df3437df1bd468fa8bcbe36f066f3a0ada45e81328e659befcc86e70"}, "downloads": -1, "filename": "z3c.formdemo-1.0.0c2.tar.gz", "has_sig": false, "md5_digest": "75d9a26ea296dffad7e25cef9c332c7f", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 54504, "upload_time": "2007-05-22T21:35:59", "upload_time_iso_8601": "2007-05-22T21:35:59Z", "url": "https://files.pythonhosted.org/packages/62/8a/3ace6873801505835191bb9ba3d733ccbd4cae800a259ce6fb4f9a3b4e9c/z3c.formdemo-1.0.0c2.tar.gz", "yanked": false}], "1.1.0": [{"comment_text": "", "digests": {"md5": "9d605bd559ea33ac57ce11f5c80fa3d3", "sha256": "06c8822bbd1aea7e2c9935c04446895c36db14130483ba3d076f8001157f8a8a"}, "downloads": -1, "filename": "z3c.formdemo-1.1.0-py2.4.egg", "has_sig": false, "md5_digest": "9d605bd559ea33ac57ce11f5c80fa3d3", "packagetype": "bdist_egg", "python_version": "2.4", "requires_python": null, "size": 98000, "upload_time": "2007-05-30T17:18:50", "upload_time_iso_8601": "2007-05-30T17:18:50Z", "url": "https://files.pythonhosted.org/packages/0a/69/51b974055eb137c2b7f4293247b71a7d1f9a092dbc3443caf957879d4f5a/z3c.formdemo-1.1.0-py2.4.egg", "yanked": false}, {"comment_text": "", "digests": {"md5": "f224a49cea737112284f74b859e3eed0", "sha256": "5a936a5867ded5193850dc5e671a36b45704aa314be8b25dc1ad60ef1404b473"}, "downloads": -1, "filename": "z3c.formdemo-1.1.0.tar.gz", "has_sig": false, "md5_digest": "f224a49cea737112284f74b859e3eed0", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 55990, "upload_time": "2007-05-30T17:18:38", "upload_time_iso_8601": "2007-05-30T17:18:38