}], "0.0.5": [{"comment_text": "", "digests": {"md5": "41252a2c1bbeb3393b4c69a27d74929c", "sha256": "48a67d12a493e041c9ad9f5acc3f6b2f0d08341723d4ccc451650e0145b28d0d"}, "downloads": -1, "filename": "point_viz-0.0.5-py3-none-any.whl", "has_sig": false, "md5_digest": "41252a2c1bbeb3393b4c69a27d74929c", "packagetype": "bdist_wheel", "python_version": "py3", "requires_python": null, "size": 567237, "upload_time": "2019-12-19T04:03:44", "upload_time_iso_8601": "2019-12-19T04:03:44.031511Z", "url": "https://files.pythonhosted.org/packages/dc/6d/a381b64d259fe0776931b9141fa0c6d1d63302257c8f0077a1a7e82f335e/point_viz-0.0.5-py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "ce98157419125e4a6fc81380179ad9fb", "sha256": "d7d81498e8c727bd278009c4efb165c7a682fe7b3245719afe6cfabfd3f39c6e"}, "downloads": -1, "filename": "point_viz-0.0.5.tar.gz", "has_sig": false, "md5_digest": "ce98157419125e4a6fc81380179ad9fb", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 561630, "upload_time": "2019-12-19T04:03:46", "upload_time_iso_8601": "2019-12-19T04:03:46.134691Z", "url": "https://files.pythonhosted.org/packages/ba/94/a0cfaba5e360f9cac3cdac74b7028557f2e57eba2af4bbdf56ae23f300f6/point_viz-0.0.5.tar.gz", "yanked": false}], "0.0.6": [{"comment_text": "", "digests": {"md5": "364e2fb63eddaf8cc4119ddef1b2bf22", "sha256": "4917ff5b36b0982d120246e11e31d3e2d9be085116770c13d008018d6a39f01f"}, "downloads": -1, "filename": "point_viz-0.0.6-py3-none-any.whl", "has_sig": false, "md5_digest": "364e2fb63eddaf8cc4119ddef1b2bf22", "packagetype": "bdist_wheel", "python_version": "py3", "requires_python": null, "size": 567232, "upload_time": "2019-12-19T04:06:56", "upload_time_iso_8601": "2019-12-19T04:06:56.310796Z", "url": "https://files.pythonhosted.org/packages/23/0c/7f23052372671c51978916addd7045e4ae9ec04c9f4ccabaeb0d67bef620/point_viz-0.0.6-py3-none-any.whl", "yanked": false}, {"comment_text": "", "digests": {"md5": "8a1b66f415cca4c9efda7e