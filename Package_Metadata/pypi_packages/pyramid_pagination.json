ilename": "pyramid_pagination-0.1.4.tar.gz", "has_sig": false, "md5_digest": "5e4659972de681a96e6e22aacdff0771", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 17233, "upload_time": "2015-08-19T18:27:42", "upload_time_iso_8601": "2015-08-19T18:27:42.794814Z", "url": "https://files.pythonhosted.org/packages/96/bb/3ce45823b6fefb9fb7f463248140c7082c18b22034bb9ff2229af4b5e5d1/pyramid_pagination-0.1.4.tar.gz", "yanked": false}], "0.1.5": [{"comment_text": "", "digests": {"md5": "1b3a698f464228f47a16126b9f328b32", "sha256": "8ab37538cd41788fe963cc6262955ac4041b28e034caed15a9b85e196003f77c"}, "downloads": -1, "filename": "pyramid_pagination-0.1.5.tar.gz", "has_sig": false, "md5_digest": "1b3a698f464228f47a16126b9f328b32", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 17196, "upload_time": "2015-12-04T16:49:45", "upload_time_iso_8601": "2015-12-04T16:49:45.848933Z", "url": "https://files.pythonhosted.org/packages/67/fe/4205c4fa8bc8488d995d9f2292b47e347c84d85f6b5d9984ef0f1189c225/pyramid_pagination-0.1.5.tar.gz", "yanked": false}], "0.1.6": [{"comment_text": "", "digests": {"md5": "20f24d4faafe91b52688f874f62c0cff", "sha256": "769ca15c1310521d9da4bf9a38feba317eda0f594ae565656879fa537d693222"}, "downloads": -1, "filename": "pyramid_pagination-0.1.6.tar.gz", "has_sig": false, "md5_digest": "20f24d4faafe91b52688f874f62c0cff", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 17267, "upload_time": "2016-04-12T21:58:51", "upload_time_iso_8601": "2016-04-12T21:58:51.683050Z", "url": "https://files.pythonhosted.org/packages/68/f2/1ebf9d557e4f000e02640d7455849c7ab86e55d1b510087f32a0dab61842/pyramid_pagination-0.1.6.tar.gz", "yanked": false}]}, "urls": [{"comment_text": "", "digests": {"md5": "20f24d4faafe91b52688f874f62c0cff", "sha256": "769ca15c1310521d9da4bf9a38feba317eda0f594ae565656879fa537d693222"}, "downloads": -1, "filename": "pyramid_pagination-0.1.6.tar.gz", "has_sig": false, "md5_digest": "20f24d4faafe91b52688f874f62c0cff", "packagetype": "sdist", "python_version": "source", "requires_python": null, "size": 17267, "upload_time": "2016-04-12T21:58:51", "upload_time_iso_8601": "2016-04-12T21:58:51.683050Z", "url": "https://files.pythonhosted.org/packages/68/f2/1ebf9d557e4f000e02640d7455849c7ab86e55d1b510087f32a0dab61842/pyramid_pagination-0.1.6.tar.gz", "yanked": false}], "timestamp": "Fri May  8 02:58:16 2020"}